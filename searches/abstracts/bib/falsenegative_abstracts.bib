Number = {2},
Pages = {151 - 173},
Title = {The error–reaction time correlation as a prediction of category verification models.},
Volume = {111},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1998-04718-001&site=ehost-live},
