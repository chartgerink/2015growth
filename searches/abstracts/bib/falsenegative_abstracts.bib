Journal = {Maternal and Child Health Journal},
Keywords = {multiple gestation groups, birth, fetal death, twins, child health, Death and Dying, Fetus, Mortality Rate, Multiple Births, Twins, Birth, Health, Pregnancy},
Number = {3},
Pages = {225 - 228},
Title = {Identifying Multiple Gestation Groups Using State-Level Birth and Fetal Death Certificate Data.},
URL = {janemlazar@yahoo.com},
Volume = {10},
Year = {2006},
}


