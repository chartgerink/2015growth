}


@article{2002-13331-00320010601,
Abstract = {Explores definitions and predictors of employee substance abuse, using 173 employed undergraduates (aged 18-52 yrs) who completed a prominent written integrity test and a self-report of work behaviors, including on-the-job (OTJ) alcohol and drug abuse. Approximately 21% of participants reported recent OTJ abuse and were significantly more likely than nonabusers to exhibit other counterproductive work behaviors. Validity coefficients for the integrity test's Employability Index and Drug Avoidance Scale suggest that such measures can reduce OTJ abuse, but the procedure's effectiveness may be limited by the high rate of false negatives. The authors discuss suggestions for improving psychological testing to reduce OTJ abuse. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
