Keywords = {neonatal screening for homocystinuria, implications for psychiatric disturbances & mental retardation, 32 yr study, Mental Disorders, Metabolism Disorders, Screening, Intellectual Development Disorder},
Number = {21},
Pages = {1572 - 1576},
Title = {Reduction of false negative results in screening of newborns for homocystinuria.},
