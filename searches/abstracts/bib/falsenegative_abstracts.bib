Keywords = {clinical quality measures, nursing homes, medicare services, medicaid services, risk adjustment, health insurance, Health Insurance, Medicaid, Medicare, Nursing Homes, Quality of Services, Risk Factors},
Number = {5},
Pages = {532 - 541},
Title = {Does risk adjustment of the CMS quality measures for nursing homes matter?},
URL = {dmukamel@uci.edu},
