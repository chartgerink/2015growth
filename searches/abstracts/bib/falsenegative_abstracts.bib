Journal = {Neuropsychobiology},
Keywords = {enhanced suppression, adrenocorticotropic hormone, cortisol response, hypothalamic pituitary adrenal function, thyrotropin releasing hormone tests, stressful life events, major depressive disorder, Corticotropin, Hypothalamic Pituitary Adrenal Axis, Major Depression, Stress, Thyrotropin, Dexamethasone, Drug Therapy, Hydrocortisone},
Number = {1},
