}


@article{1989-07955-00119880101,
Abstract = {16 Native French speakers and 16 American English speakers followed the reading of a poem from sheets that contained either the usual printed poetic format or a margin-to-margin, upper case, unpunctuated format. Results indicate 114 false positives, 163 false negatives, and 189 correctly reported pauses. Short pauses accounted for 96% of false negatives. English speakers were facilitated by the poetic format, whereas French speakers were inhibited by it. It is argued that using pause reports as veridical data is methodologically unsound. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
