Keywords = {cardiovascular pre-participation screening, young competitive athletes, prevention, sudden death, Athletes, Cardiovascular Disorders, Competition, Death and Dying, Screening, Prevention},
Number = {4},
Pages = {227 - 233},
Title = {Cardiovascular pre-participation screening of young competitive athletes for prevention of sudden death in China.},
URL = {jinanzh505@yahoo.com},
Volume = {10},
Year = {2007},
}


