Keywords = {response, remission, major depressive disorder, threshold, misclassification bias, Major Depression, Rating Scales, Remission (Disorders), Thresholds, Rating, Responses},
Number = {4},
Pages = {469 - 473},
Title = {The fallacy of thresholds used in defining response and remission in depression rating scales.},
URL = {floriannaudet@gmail.com},
