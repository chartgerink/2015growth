Journal = {Human Factors},
Keywords = {model for human vs automation resource allocation decision making criterion for failure-detection task, false negative & false positive detection error responses, Automation, Decision Making, Human Machine Systems, Mathematical Modeling, Signal Detection (Perception), Costs and Cost Analysis, Errors, Resource Allocation},
Number = {3},
Pages = {403 - 407},
Title = {Human versus automation in responding to failures: An expected-value analysis.},
