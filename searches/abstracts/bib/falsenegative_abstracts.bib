Number = {Suppl},
Pages = {493 - 498},
Title = {Screening for speech and language impairment: A follow-up of true negatives and false positives.},
Volume = {36},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2001-06437-087&site=ehost-live},
