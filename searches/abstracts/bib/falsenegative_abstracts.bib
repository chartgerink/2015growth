Keywords = {brain injury, disease severity, magnetic resonance imaging, imagery, blood, oxygenation, Blood, Brain Damage, Oxygenation, Severity (Disorders), Functional Magnetic Resonance Imaging, Imagery},
Number = {2},
Pages = {158 - 160},
Title = {Wait, wait…Don't tell me.},
URL = {jjfins@med.cornell.edu},
Volume = {69},
Year = {2012},
}


@article{2012-02855-00620111201,
Abstract = {The term "cognitive distortion" is defined as "incorrect" information processing. These "deformed" thoughts lead the individual to reach negative and false conclusions, as in the case of pathological gambling. Before and during gambling, gamblers present certain cognitive distortions which make them think in a particular way and encourage them to continue to gamble. In this article, the authors study the illusion of control, superstition and predictive control. They also discuss the various psychometric tools available to evaluate cognitive distortions during the management of pathological gambling. This article demonstrates the value of analysis of cognitive distortions in the management of pathological gambling, particularly in cognitive and behavioural therapy. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
