Journal = {The International Journal of Biostatistics},
Keywords = {medical decision making, prognostic factors, risk assessment, risk management, kidney graft failure, cut-off estimation, Decision Making, Organ Transplantation, Prognosis, Risk Management, Risk Assessment, Kidneys},
Number = {1},
Pages = {1 - 13},
Title = {Cut-Off estimation and medical decision making based on a continuous prognostic factor: The prediction of kidney graft failure.},
Volume = {8},
