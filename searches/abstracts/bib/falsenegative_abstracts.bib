Year = {1997},
}


@article{2003-01360-00620021001,
Abstract = {We share their points of agreement on the field of early intervention in schizophrenia. We concur that optimal treatment interventions should begin as early in fully declared psychosis as possible. Although we differ in our interpretation of the duration of untreated psychosis (DUP) research, we concur that there are many other reasons to intervene early in psychotic disorders. We recognize that post-illness early intervention efforts are categorically different from interventions which precede the emergence of the full psychotic syndrome, and that the latter pose potential problems of negative consequences for false-positive cases. We do not believe, given the current stage of knowledge, that it is appropriate to apply early intervention strategies to asymptomatic subjects or prepsychotic cases who are not seeking help. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
