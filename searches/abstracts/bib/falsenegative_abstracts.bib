Keywords = {mental illness, police cells, police processes, brief jail, mental health, jail screening assessment tool, psychometrics, test validity, test reliability, Mental Disorders, Prisons, Psychometrics, Test Reliability, Test Validity, Mental Health, Police Personnel, Screening},
Number = {6},
Pages = {529 - 542},
Title = {Identification of mental illness in police cells: A comparison of police processes, the Brief Jail Mental Health Screen and the Jail Screening Assessment Tool.},
URL = {stuart.thomas@med.monash.edu.au},
Volume = {18},
Year = {2012},
