Number = {2},
Pages = {139 - 145},
Title = {Implications of β₁- and β₂-adrenergic receptors in the antinociceptive effect of tricyclic antidepressants.},
Volume = {7},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1997-06584-009&site=ehost-live},
Year = {1997},
}


@article{1997-06505-00719970401,
Abstract = {Examined the applicability of a 2-stage procedure in determining the prevalence of personality disorders (PD) in the general population. 258 college students (aged 18–19 yrs) servved as Ss. Ss were interviewed with the International Personality Disorder Examination (IPDE) and Structured Clinical Interview for the Diagnostic and Statistical Manual of Mental Disorders-III—Revised: Nonpatient Version (SCID-P) following initial screening to differentiate broadly between those Ss who might have a clinically significant psychiatric disorder from those who do not. The screen failed to miss anyone who was found to have a definite PD, when examined by experienced clinicians with a reliable semistructured interview. Efforts to reduce the number of false-positive cases by raising the screening threshold resulted in a significant increase in the number of false-negative results. The screen missed 9% of those who were included in the combined definite or probable category. Compared with the clinicians the screen tended to be conservative in making diagnosis. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Lenzenweger, Mark F. and  Loranger, Armand W. and  Korfine, Lauren and  Neff, Cynthia},
ISSN = {0003-990X, 1538-3636},
