ISSN = {0028-2804, 1433-0407},
Journal = {Der Nervenarzt},
Keywords = {acute stroke patients, quality of diagnosis, emergency medical services, treatment evaluation, Cerebrovascular Accidents, Emergency Services, Quality of Care, Treatment Effectiveness Evaluation},
Number = {6},
Pages = {759 - 765},
Title = {Zuweisungskonzept bei akutem schlaganfall: Untersuchung der rettungsdienstlichen diagnosequalität in der stadt Münster. = Concept for allocation of acute stroke patients. Evaluation of the quality of diagnosis reached by the emergency medical services of Münster.},
