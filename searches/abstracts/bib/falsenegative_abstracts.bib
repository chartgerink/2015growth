Journal = {Evaluation Review},
Keywords = {evaluation of community based substance abuse prevention programs, Community Services, Drug Abuse Prevention, Program Evaluation},
Number = {1},
Pages = {78 - 94},
Title = {Zero effects in substance abuse programs: Avoiding false positives and false negatives in the evaluation of community–based programs.},
