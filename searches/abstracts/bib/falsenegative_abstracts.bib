Pages = {295 - 302},
Title = {The Utility of the Rorschach Coping Deficit Index as a Measure of Depression and Social Skills Deficits in Children and Adolescents.},
Volume = {12},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2005-09187-005&site=ehost-live},
