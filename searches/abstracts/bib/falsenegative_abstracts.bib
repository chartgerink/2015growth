Title = {'A non-standard method for estimating accuracy of lie detection techniques demonstrated on a self-validating set of field polygraph examinations': Corrigendum.},
Volume = {20},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2013-43345-006&site=ehost-live},
