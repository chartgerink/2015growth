Keywords = {candidate measures & trade-offs in screening & determination of potency of client risk factors for clinical & policy significance, At Risk Populations, Decision Making, Health Screening, Measurement, Risk Assessment, Client Characteristics, Clinical Judgment (Not Diagnosis), Health Care Policy},
Number = {3},
Pages = {257 - 271},
Title = {Measuring the potency of risk factors for clinical or policy significance.},
