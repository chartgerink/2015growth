URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1982-27255-001&site=ehost-live},
Year = {1980},
}


@article{1982-22385-00119811201,
Abstract = {Discusses the relationship between changes in delinquency prevalence over time and prediction error. Published data from a 21-yr British longitudinal study by M. E. Wadsworth (1978–1979) are used to exemplify the formal relationships that are derived. Statistically, as prevalence increases, there will be a relative increase in false negative errors and a decrease in false positive errors; the relationship is independent of prediction accuracy. Substantively, this means that as delinquency prevalence increases, imperfect prediction models will move toward "missing" more actual delinquents (increasing false negatives) and "mistaking" fewer actual nondelinquents (decreasing false positives). Some characterizations of differentially weighting the costs of false negative and false positive errors are presented in terms of intervention and nonintervention policies and decisions. (33 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Dunn, Christopher S.},
ISSN = {0093-8548, 1552-3594},
Journal = {Criminal Justice and Behavior},
Keywords = {prediction error in longitudinal studies of delinquency prevalence, Epidemiology, Experimentation, Juvenile Delinquency, Prediction Errors},
Number = {4},
Pages = {439 - 470},
Title = {Changes in delinquency prevalence: Prediction problems and decision logic in longitudinal studies of delinquency.},
