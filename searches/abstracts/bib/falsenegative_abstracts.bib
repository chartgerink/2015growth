@article{1997-05328-00419970701,
Abstract = {Determined the accuracy of the clinical diagnosis of Pick's disease by presenting 105 autopsy-confirmed cases of Pick's (n =  7) and related disorders (n =  98) as clinical vignettes in randomized order to 6 neurologists who were unaware of the autopsy findings. The group of raters had moderate to fair agreement for the diagnosis of Pick's as measured by the κ statistics. The sensitivity for the diagnosis of Pick's for the 1st visit and last visit was low (0–71%), but specificity was near-perfect. Median positive predictive values at both visits were 83–85%. False-negative misdiagnoses mainly involved Alzheimer's disease (AD). False-positive diagnoses were rare and occurred with corticobasal degeneration (1st visit) and with dementia with Lewy bodies (last visit). Pick's was also misdiagnosed by primary neurologists. The best clinical predictors for the early diagnosis of Pick's included, frontal dementia, early cortical dementia with severe frontal lobe disturbances, absence of apraxia, and absence of gait disturbance at onset. However, the 1st neurologic evaluation in some of the Pick's cases took place in advanced stages of the disease. Findings suggest that this disorder is underdiagnosed in clinical practice. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Litvan, I. and  Agid, Y. and  Sastrj, N. and  Jankovic, J. and  Wenning, G. K. and  Goetz, C. G. and  Verny, M. and  Brandel, J. P. and  Jellinger, K. and  Chaudhuri, K. R. and  McKee, A. and  Lai, E. C. and  Pearce, R. K. and  Bartko, J. J.},
ISSN = {0028-3878, 1526-632X},
Journal = {Neurology},
Keywords = {early predictors for & accuracy of diagnosis of Pick's disease, neurologists, Medical Diagnosis, Neurologists, Picks Disease},
Number = {1},
