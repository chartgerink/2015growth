Keywords = {diagnostic value of MRI &/or technetium 99m hexamethyl-propyleneamine oxide SPECT for Alzheimer's diease, 67–86 yr olds with probable dementia, Netherlands, Alzheimer's Disease, Dementia, Magnetic Resonance Imaging, Medical Diagnosis, Tomography},
Number = {2},
Pages = {63 - 70},
Title = {The diagnostic value of magnetic resonance imaging and technetium 99m-HMPAO single-photon-emission computed tomography for the diagnosis of Alzheimer disease in a community-dwelling elderly population.},
