ISSN = {0081-685X},
Journal = {Studia Psychologiczne},
Keywords = {control deprivation, prejudice, stereotypic information processing, Gipsy persons, Impression Formation, Prejudice, Recognition (Learning), Stereotyped Attitudes, Romanies},
Number = {1},
Pages = {133 - 158},
Title = {Poziom upredzeń, deprywacja kontroli: A Stereotypowe przetwarzanie informacji na temat cyganów. = The level of prejudice, control deprivation, and stereotypic information processing about Gipsy persons.},
