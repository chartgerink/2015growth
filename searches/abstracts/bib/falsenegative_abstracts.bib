Keywords = {small fiber neuropathy, NeuroQuick, screening tool, neuropathic pain, Neuropathology, Pain, Screening Tests},
Number = {8},
Pages = {1000 - 1007},
Title = {NeuroQuick--A novel bedside test for small fiber neuropathy?},
URL = {ida@haussleiter.de},
