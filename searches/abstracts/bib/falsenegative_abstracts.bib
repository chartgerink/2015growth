ISSN = {0275-5408, 1475-1313},
Journal = {Ophthalmic and Physiological Optics},
Keywords = {contrast sensitivity function results measured with adaptive probit estimation vs with method of constant stimuli & single staircase procedures, spatial frequency dependent O bias, 21–60 yr olds, Psychometrics, Response Bias, Spatial Frequency, Stimulus Presentation Methods, Visual Contrast},
