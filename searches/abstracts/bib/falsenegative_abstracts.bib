Title = {The staircase test: Some evidence of nonspecificity for anxiolytics.},
Volume = {89},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1987-24477-001&site=ehost-live},
Year = {1986},
}


