Number = {6},
Pages = {498 - 505},
Title = {Screening for late life depression: Cut-off scores for the Geriatric Depression Scale and the Cornell Scale for Depression in Dementia among Japanese subjects.},
URL = {a-streit@jrckicn.acjp},
Volume = {18},
Year = {2003},
