@article{1991-15505-00119900601,
Abstract = {The disease construction of schizophrenia (SZ) originated during a period of rapid growth of biological science based on mechanistic principles. Crude diagnostic measures failed to differentiate absurd, unwanted conduct due to biological conditions from atypical conduct directed to solving existential or identity problems. The patient came to be regarded as an object without agency or goals. In spite of enormous research funding, no biological or psychological marker has been discovered that would differentiate diagnosed SZs from normals without creating unacceptable proportions of false positives and false negatives. It is concluded that SZ is an obsolescent hypothesis and should be abandoned. An alternate framework, contextualism, is discussed in which persons are perceived as agents, actors, or performers. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Sarbin, Theodore R.},
ISSN = {0271-0137},
