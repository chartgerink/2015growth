Year = {1997},
}


@article{1997-06735-00519970601,
Abstract = {The results of many human operant conditioning experiments appear to show that humans are less sensitive than are nonhumans to operant consequences, which suggests species discontinuities in basic behavioral processes. A reanalysis of 311 data sets from 25 studies that employed variable-interval schedules of reinforcement designed to assess sensitivity to reinforcement corroborates the claim that human behavioral allocation among alternatives often deviates from predictions based on rates of experimentally programed consequences. Close inspection of the studies in question, however, suggests that methodological issues contribute heavily to the differences noted so far between humans and nonhumans and that an explanation based upon species discontinuities is not tenable. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
