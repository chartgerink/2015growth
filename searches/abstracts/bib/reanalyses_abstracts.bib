Keywords = {cross linguistic validity, electrophysiology, morphosyntactic processing, agreement violations, Basque, case agreement, verb agreement, sentence processing, event related potentials, Electrophysiology, Evoked Potentials, Linguistics, Syntax, Verbs, Morphology (Language)},
Number = {3},
Pages = {357 - 373},
Title = {On the cross-linguistic validity of electrophysiological correlates of morphosyntactic processing: A study of case and agreement violations in Basque.},
URL = {diaz@cbs.mpg.de},
Volume = {24},
Year = {2011},
}


@article{2011-03177-00720110501,
Abstract = {While it is widely acknowledged in the formal semantic literature that both the truth-functional focus particle only and it-clefts convey exhaustiveness, the nature and source of exhaustiveness effects with it-clefts remain contested. We describe a questionnaire study (n = 80) and an event-related brain potentials (ERP) study (n = 16) that investigated the violation of exhaustiveness in German only-foci versus it-clefts. The offline study showed that a violation of exhaustivity with only is less acceptable than the violation with it-clefts, suggesting a difference in the nature of exhaustivity interpretation in the two environments. The ERP-results confirm that this difference can be seen in online processing as well: a violation of exhaustiveness in only-foci elicited a centro-posterior positivity (600–800ms), whereas a violation in it-clefts induced a globally distributed N400 pattern (400–600ms). The positivity can be interpreted as a reanalysis process and more generally as a process of context updating. The N400 effect in it-clefts is interpreted as indexing a cancelation process that is functionally distinct from the only case. The ERP study is, to our knowledge, the first evidence from an online experimental paradigm which shows that the violation of exhaustiveness involves different underlying processes in the two structural environments. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Drenhaus, Heiner and  Zimmermann, Malte and  Vasishth, Shravan},
ISSN = {0911-6044},
Journal = {Journal of Neurolinguistics},
Keywords = {exhaustiveness, clefts, truth-functional, semantics, event related potentials, language, electrophysiology, Electrophysiology, Evoked Potentials, Language, Semantics},
Number = {3},
Pages = {320 - 337},
Title = {Exhaustiveness effects in clefts are not truth-functional.},
URL = {h.drenhaus@gmail.com, mazimmer@uni-potsdam.de, vasishth@uni-potsdam.de},
Volume = {24},
Year = {2011},
}


@article{2011-10397-00820110501,
Abstract = {Three experiments examined the influence of first lexeme morphological family size on English compound word recognition. Concatenated compound words whose first lexemes were from large morphological families were responded to faster in word naming and lexical decision than compounds from small morphological families. In addition, an eye movement experiment showed that gaze durations were shorter on compounds from large morphological families during sentence reading. This was mainly due to more refixations on compounds from small morphological families. Posthoc analyses and reanalysis of past studies suggested that compounds with a larger number of higher frequency family members (HFFM) are read more slowly than compounds with fewer HFFM. Thus, while morphological family size is generally facilitative, the presence of HFFM has an inhibitory effect on eye movement behaviour. The time-course of these effects is discussed. (PsycINFO Database Record (c) 2014 APA, all rights reserved). (journal abstract)},
Author = {Juhasz, Barbara J. and  Berkowitz, Rachel N.},
ISSN = {0169-0965, 1464-0732},
Journal = {Language and Cognitive Processes},
Keywords = {morphological families, English compound word recognition, lexical decision, eye movements, Eye Movements, Lexical Decision, Morphology (Language), Word Recognition, Words (Phonetic Units)},
Number = {4-6},
Pages = {653 - 682},
Title = {Effects of morphological families on English compound word recognition: A multitask investigation.},
URL = {bjuhasz@wesleyan.edu},
Volume = {26},
Year = {2011},
}


@article{2011-05449-00120110418,
Abstract = {Deteriorated phonological representations are widely assumed to be the underlying cause of reading difficulties in developmental dyslexia; however, existing evidence also implicates degraded orthographic processing. Here, we used event-related potentials whilst dyslexic and control adults performed a pseudoword-word priming task requiring deep phonological analysis to examine phonological and orthographic priming, respectively. Pseudowords were manipulated to be homophonic or non-homophonic to a target word and more or less orthographically similar. Since previous ERP research with normal readers has established phonologically driven differences as early as 250 ms from word presentation, degraded phonological representations were expected to reveal reduced phonological priming in dyslexic readers from 250 ms after target word onset. However, phonological priming main effects in both the N2 and P3 ranges were indistinguishable in amplitude between groups. Critically, we found group differences in the N1 range, such that orthographic modulations observed in controls were absent in the dyslexic group. Furthermore, early group differences in phonological priming transpired as interactions with orthographic priming (in P2, N2 and P3 ranges). A group difference in phonological priming did not emerge until the P600 range, in which the dyslexic group showed significantly attenuated priming. As the P600 is classically associated with online monitoring and reanalysis, this pattern of results suggest that during deliberate phonological processing, the phonological deficit in reading may relate more to inefficient monitoring rather than deficient detection. Meanwhile, early differences in perceptual processing of phonological information may be driven by the strength of engagement with orthographic information. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Savill, Nicola J. and  Thierry, Guillaume},
ISSN = {0006-8993},
Journal = {Brain Research},
Keywords = {dyslexia, orthography, phonological integration deficits, auditory stimulation, priming, Auditory Stimulation, Dyslexia, Orthography, Phonology, Evoked Potentials, Priming},
Pages = {192 - 205},
Title = {Reading for sound with dyslexia: Evidence for early orthographic and late phonological integration deficits.},
URL = {n.j.savill@bangor.ac.uk},
Volume = {1385},
Year = {2011},
}


@article{2011-06402-00220110401,
Abstract = {Visual search involves a subtle coordination of visual memory and lower-order perceptual mechanisms. Specifically, the fluctuations in gaze may provide support for visual search above and beyond what may be attributed to memory. Prior research indicates that gaze during search exhibits fractal fluctuations, which allow for a wide sampling of the field of view. Fractal fluctuations constitute a case of fast diffusion that may provide an advantage in exploration. We present reanalyses of eye-tracking data collected by Stephen and Mirman (Cognition, 115, 154–165, 2010) for single-feature and conjunction search tasks. Fluctuations in gaze during these search tasks were indeed fractal. Furthermore, the degree of fractality predicted decreases in reaction time on a trial-by-trial basis. We propose that fractality may play a key role in explaining the efficacy of perceptual exploration. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Stephen, Damian G. and  Anastas, Jason},
ISSN = {1943-3921, 1943-393X},
Journal = {Attention, Perception, & Psychophysics},
Keywords = {fractal fluctuations, gaze speed, visual search, Eye Fixation, Visual Memory, Visual Search},
Number = {3},
Pages = {666 - 677},
Title = {Fractal fluctuations in gaze speed visual search.},
URL = {damian.stephen@wyss.harvard.edu},
Volume = {73},
Year = {2011},
}


@article{2011-00052-01420110401,
Abstract = {Reanalysis of data (N = 512) of Van der Linden, Scholte, Cillessen, Te Nijenhuis, and Segers (2010) does not provide support for the existence of a General Factor of Personality (GFP) and of its relation with likeability and popularity among adolescent classmates. First of all, it was impossible to fit a GFP model to the personality data without setting the error variance associated with Extraversion to zero and thus aligning it exclusively with Extraversion. Second, in the full model, which included likeability and popularity, a model without GFP showed a better fit than the Van der Linden et al. (2010) GFP model. Third, when entered together with Extraversion, the GFP did not add any additional variance in likeability and even acted as an suppressor variable of the relation between Extraversion and popularity. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
