Keywords = {multidimensional scaling vs factor analysis, differentiation of ability test items requiring rule inference vs application, Aptitude Measures, Factor Analysis, Inductive Deductive Reasoning, Item Analysis (Statistical), Multidimensional Scaling},
Number = {2},
Pages = {308 - 311},
Title = {Inductive and deductive reasoning: A structural reanalysis of ability tests.},
Volume = {73},
