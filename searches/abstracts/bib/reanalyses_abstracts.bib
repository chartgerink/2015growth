Journal = {Neurological Sciences},
Keywords = {A-wave, clinical meaning, Guillain–Barre syndrome, late motor responses, polyradiculoneuropathy, Electrical Activity, Nervous System Disorders, Somatosensory Evoked Potentials, Guillain-Barre Syndrome},
Number = {5},
Pages = {609 - 616},
Title = {Further insight on A-wave in acute and chronic demyelinating neuropathies.},
