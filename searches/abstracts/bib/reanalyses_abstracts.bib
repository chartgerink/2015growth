}


@article{2002-00580-00420020101,
Abstract = {Previous studies have reported an interaction between visual field (VF) and word length such that word recognition is affected more by length in the left VF (LVF) than in the right VF (RVF). A reanalysis showed that the previously reported effects of length were confounded with orthographic neighborhood size (NS). In 3 experiments the authors manipulated length and NS in lateralized lexical decision tasks. 24 Ss (aged 18-29 yrs; Exp 1), 24 Ss (aged 18-25 yrs; Exp 2), and 24 Ss (aged 18-26 yrs; Exp 2) participated in the study. Results showed that length and VF interacted even with NS controlled (Exp 1); that NS affected responses to words in the LVF but not the RVF (Exp 2); and that when length and NS were combined, length only affected performance in the LVF for words with few neighbors. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Lavidor, Michal and  Ellis, Andrew W.},
ISSN = {0093-934X},
Journal = {Brain and Language},
Keywords = {word length, orthographic neighborhood size, word recognition, visual field, cerebral hemispheres, visual lexical decision, hemispheric differences, lateralization, Lateral Dominance, Lexical Decision, Orthography, Visual Field, Words (Phonetic Units), Cerebral Cortex},
Number = {1},
