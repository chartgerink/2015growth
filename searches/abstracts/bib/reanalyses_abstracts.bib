Pages = {129 - 130},
Title = {A presenilin 1 mutation in the first case of Alzheimer's disease.},
URL = {ulrich.mueller@med.uni-giessen.de},
Volume = {12},
Year = {2013},
}


@article{2013-16351-01920130101,
Abstract = {Comments on the articles by R. D. Gibbons et al. (see record [rid]2013-03110-011[/rid]) and R. D. Gibbons et al. (see record [rid]2013-03110-010[/rid]). Gibbons et al. the black box warning about antidepressant drugs and suicide risk, Gibbons et al conducted risk/efficacy reanalysis of selected data sets. These conclusions are unsound. The data are contaminated by trial LYAQ, which studied atomoxetine hydrochloride with or without fluoxetine in patients with attention-deficit/hyperactivity disorder with depressive or anxious symptoms. Less than half had major depression diagnoses; pretreatment depression scores matched posttreatment scores in the other youth studies. These problematic details were not addressed. By lack of conforming diagnoses and design, LYAQ outright invalidates the aggregate analyses. These deeply flawed reports do not justify either questioning or affirming the black box warning: They simply cannot inform the debate. The Nobelist Peter Medawar remarked on the futility of trying to answer scientific questions that are beyond the reach of available methods or data sets. The inadequate methods and samples of these reports make Medawar’s point. It is time to move on to properly designed and clinically grounded prospective studies. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
