@article{1997-04417-00519970301,
Abstract = {Configural frequency analysis (CFA) tests whether certain individual patterns in different variables are observed more frequently in a sample than expected by chance. In normative CFA, these patterns are derived from the S's specific position in relation to sample characteristics such as the median or the mean. In ipsative CFA, patterns are defined within an individual reference system, e.g., relative to the S's median of different variable scores. Normative CFA examines dimensionality of scales and is comparable to factor analysis in this respect. Ipsative CFA rather yields information about location of scores in different variables, in a similar way to ANOVA or Friedman testing. However, both normative and ipsative CFA may supply information not obtainable by means of the aforementioned methods. This is illustrated in a reanalysis of data in 4 scales of an anxiety inventory. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Köhler, Thomas and  Stemmler, Mark},
ISSN = {0890-2070, 1099-0984},
Journal = {European Journal of Personality},
Keywords = {use of normative vs ipsative configural frequency analysis in personality research, situation anxiety, Anxiety, Frequency Distribution, Personality Measures, Statistical Analysis},
Number = {1},
Pages = {69 - 80},
Title = {Normative versus ipsative configural frequency analysis in personality research—their use discussed in a reanalysis of data on situation-bound anxiety.},
Volume = {11},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1997-04417-005&site=ehost-live},
