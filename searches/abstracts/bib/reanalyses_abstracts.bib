}


@article{2001-06095-00220011201,
Abstract = {Examined the readability of an existing stroke brochure and illustrated how the readability of patient education materials may be improved. Stroke education information from the Australian National Stroke Foundation was assessed for readability using three formulas: Flesch reading ease, Statistical Measure of Gobbledegook (SMOG), and the Fry readability graph. A Flesch reading ease score indicated that the brochure was in the "standard" to "fairly difficult" reading range. The results of the Fry and SMOG index assessments indicated that a reading level of 10th and 11th grades were required to comprehend the brochure information. Revision of a randomly selected portion of text using established guidelines for writing patient information, and subsequent reanalysis, showed that simple revisions to health literature can dramatically improve readability. For example, the Flesch reading ease score of the original version of the text required a reading grade level of 12; in the revised version a grade level of 5 was required. Given the importance of effective communication of prevention information about stroke, these results are presented as a model of how future revisions of difficult to read stroke-related literature might be conducted to improve public education. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Sullivan, Karen and  O'Conor, Frances},
