Title = {J. P. Rushton's aggregational errors in racial psychology.},
Volume = {19},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1994-00837-001&site=ehost-live},
Year = {1993},
}


@article{1994-00269-00119930701,
Abstract = {Indicates that the Gaussian, or "normal," distribution is routinely used to model distributions of subjective quantities. This practice rests on a trust in the central limit theorem; however, this theorem does not cover the case of a mixture of Gaussian distributions with different standard deviations. In psychophysical judgment experiments, this may result from fluctuating or interindividually varying attention. Two candidates for describing this type of distribution are the logistic distribution and the t distribution with a small number of degrees of freedom. In reanalyses of A. Hellström's experimental data (see record [rid]1980-27097-001[/rid]) on 3-category loudness comparisons, and in a Monte Carlo simulation, t(4) was found to model the underlying mixed inter- and intraindividual distribution of subjective loudness differences satisfactorily. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Hellström, Åke},
ISSN = {0031-5117, 1532-5962},
Journal = {Perception & Psychophysics},
Keywords = {Gaussian normal distribution to model subjective quantities in psychophysical judgment experiments, Experimental Methods, Normal Distribution, Psychophysical Measurement},
Number = {1},
Pages = {82 - 92},
Title = {The normal distribution in scaling subjective stimulus differences: Less 'normal' than we think?},
