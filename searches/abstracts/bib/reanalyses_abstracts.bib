Year = {1997},
}


@article{1997-43435-00119970101,
Abstract = {Uses a developmental function approach to describe age-related change and individual differences in infant information processing during the 1st yr of life. The Visual Expectation Paradigm is used to measure speed of information processing, response variability, and expectancy formation. Eye-movement reaction time and anticipatory saccades were gathered for 13 infants assessed monthly from 2–9 mo and then again at 12 mo. Analysis of response patterns demonstrated the applicability of the paradigm throughout the age range studied. Converging operations strongly indicate that the traditional estimate of the minimum time required for infants to initiate a saccade to a peripheral stimulus may be as much as 100 msec too long. Moreover, the newly estimated minimum of 133 msec does not appear to change during 2–12 mo period. Reanalysis of the present data and past research reveals that the new, shorter minimum reaction time (RT) is unlikely to affect findings based on mean RT. However, using the traditional minimum RT will inflate estimates of percentage anticipation, especially in infants older than 5 mo. In conclusion, the Variable Criterion Model (G. R. Grice, 1968) is used to integrate 3 key findings: regular age changes in mean RT and variability but no age changes in the minimum RT. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Canfield, Richard L. and  Smith, Elliott G. and  Brezsnyak, Michael P. and  Snow, Kyle L.},
ISSN = {0037-976X, 1540-5834},
