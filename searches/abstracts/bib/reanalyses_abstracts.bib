ISSN = {0031-5117, 1532-5962},
Journal = {Perception & Psychophysics},
Keywords = {under- vs overconfidence of judgments in easy vs hard sensory discrimination, humans (mean age 31 yrs), comparison of data in Canada vs Sweden, Cross Cultural Differences, Judgment, Perceptual Discrimination, Self Confidence, Task Complexity},
