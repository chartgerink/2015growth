Keywords = {span memory, immediate memory, general ability, intelligence, best evidence approach, factor analysis, structural equation modeling, Cognitive Ability, Factor Analysis, Intelligence, Short Term Memory, Structural Equation Modeling, Factor Structure},
Number = {6},
Pages = {607 - 619},
Title = {A reappraisal of the relationship between span memory and intelligence via 'best evidence synthesis'.},
