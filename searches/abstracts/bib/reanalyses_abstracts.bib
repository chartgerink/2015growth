@article{2001-11446-00220010101,
Abstract = {Eye fixations were recorded to assess whether, how, and when readers draw inferences about predictable events. Predicting context sentences, or non-predicting control sentences, were presented to 40 college students, followed by continuation sentences in which a target word referred to a predictable event (inferential word) or an unlikely event (non-predictable word). Results show that there were no effects on initial target word processing measures, such as launch and landing sites, fixation probability, first-fixation duration, or first-pass reading time. However, relative to the control condition, the predicting context (1) speeded up reanalysis of the inferential word, as revealed by a reduction in second-pass reading time and regressions, and (2) interfered with processing of the non-predictable word, as shown by an increase in regressions. These results indicate that predictive inferences are active at late text integration processes, rather than at early lexical-access processes. The pattern of findings suggests that these inferences involve initial activation of rather general concepts following the inducing context, and that they are completed or refined with delay, after the inferential target word is read. Examples of experimental passages are appended. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Calvo, Manuel G. and  Meseguer, Enrique and  Carreiras, Manuel},
ISSN = {0340-0727, 1430-2772},
Journal = {Psychological Research},
Keywords = {eye movements, time course, predictive inferences, reading, information processing, Cognitive Processes, Eye Movements, Inference, Reading, Time, Prediction},
Number = {3},
Pages = {158 - 169},
Title = {Inferences about predictable events: Eye movements during reading.},
Volume = {65},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2001-11446-002&site=ehost-live},
