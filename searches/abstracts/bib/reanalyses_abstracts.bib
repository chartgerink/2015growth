@article{1982-20309-00119820401,
Abstract = {Recorded eye movements as 16 undergraduate Ss read sentences containing temporary structural ambiguities. In accord with the garden-path theory of sentence comprehension, shorter reading times were found for sentences conforming to certain independently motivated parsing strategies (late closure and minimal attachment) than for comparable sentences that violate these strategies. Longer fixation durations were associated with the 1st fixation in the region of the sentence that disambiguated the sentence, suggesting that the human sentence-parsing mechanism operates in a rather systematic fashion, immediately computing the structural consequences of fixated material for the analysis of preceding material. The pattern of regressive eye movements indicated that the parsing mechanism typically engages in selective reanalysis, exploiting whatever information it has available about the type of error it has committed to guide its reanalysis attempts. Understanding the parser's revision procedures is essential to an explanation of why certain linguistic structures cannot be successfully parsed by humans. (15 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Frazier, Lyn and  Rayner, Keith},
ISSN = {0010-0285},
Journal = {Cognitive Psychology},
Keywords = {sentence structural ambiguity, eye movements during reading & comprehension, college students, Eye Movements, Sentence Comprehension, Sentence Structure, Stimulus Ambiguity},
Number = {2},
