Title = {A readability analysis of Australian stroke information.},
Volume = {7},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2001-06095-002&site=ehost-live},
Year = {2001},
}


@article{2001-05318-01120011201,
Abstract = {J. Pratt, T. M. Spalek, and F. Bradshaw (1999) recently proposed that attentional momentum is the mechanism underlying the inhibition of return (IOR) effect. They suggested that momentum associated with an attentional movement away from a peripherally cued location and toward an uncued opposite location is essential and fundamental to the finding of an IOR effect. Although it is clear from the present study and from a reanalysis of data from Pratt et al. that response time can be facilitated at an uncued opposite location, this putative effect of attentional momentum is neither robust nor reliable. First, it occurs for only a minority of participants. Second, it occurs in only a subset of the cued display positions. And finally, it is uncorrelated with the occurrence of IOR. Together the data indicate that the attentional momentum hypothesis is an overgeneralization and that it does not underlie the robust and reliable IOR effect. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Snyder, Janice J. and  Schmidt, William C. and  Kingstone, Alan},
ISSN = {0096-1523, 1939-1277},
Journal = {Journal of Experimental Psychology: Human Perception and Performance},
Keywords = {attentional momentum, inhibition of return, cued vs uncued locations, response time, Attention, Cues, Reaction Time, Response Duration},
Number = {6},
Pages = {1420 - 1432},
Title = {Attentional momentum does not underlie the inhibition of return effect.},
URL = {snyderjj@mail.med.upenn.edu},
Volume = {27},
Year = {2001},
}


@article{2001-09006-00920011001,
Abstract = {Anxiety sensitivity is the fear of anxiety-related sensations, and is measured by the 16-item Anxiety Sensitivity Index (ASI; S. Reiss et al, 1986). Despite the popularity and utility of the ASI in research, studies have provided evidence for the inadequacy of several items, and item-to-scale correlations for the ASI have not been published. In this study, a converging set of analyses to evaluate the item adequacy and factor structure of the ASI was used. Ss were 340 psychiatric outpatients and 50 nonpatients. Results of these multiple analyses converged nicely suggesting that Items 1, 5, 7, 8, and 13 should be considered for removal from the instrument. The impact of removing these problematic items from the scale was explored through the reanalysis of data from 3 studies (S. G. Ball et al, 1995; M. Eke and R. J. McNally, 1996; and N. B. Schmidt et al, 1997) that compared the original ASI with the new 11-item version (the ASI minus the 5 problematic items). Results suggest that the 2 scales function comparably in many respects but that the new version may be a more precise measure of anxiety sensitivity. The 11-item ASI appears to tap 2 primary aspects of anxiety sensitivity: fears of somatic sensations of anxiety and fears of loss of mental control. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Blais, Mark A. and  Otto, Michael W. and  Zucker, Bonnie G. and  McNally, Richard J. and  Schmidt, Norman B. and  Fava, Maurizio and  Pollack, Mark H.},
ISSN = {0022-3891, 1532-7752},
Journal = {Journal of Personality Assessment},
Keywords = {item analysis, Anxiety Sensitivity Index, factor structure, psychiatric outpatients & nonpatients, Anxiety, Factor Structure, Item Analysis (Test), Psychiatric Patients, Rating Scales, Anxiety Sensitivity},
Number = {2},
Pages = {272 - 294},
Title = {The Anxiety Sensitivity Index: Item analysis and suggestions for refinement.},
URL = {mblais@partners.org},
Volume = {77},
Year = {2001},
}


@article{2001-05272-01820011001,
Abstract = {Developed a brief form of the Life Skills Profile (LSP) that incorporates all 5 subscales of the full form. A new short form of the LSP (LSP-20) was developed to incorporate all 5 subscales of the full form. The LSP-20 development was based on a reanalysis of data from previously published studies. These data sets were also reanalyzed to determine any differential effects of numbers and percentages of items in the LSP-39, LSP-16 and LSP-20, comparability of scores of the different forms, of test-retest and interrater reliability, and validity of the LSP-20 by comparison with the Positive and Negative Syndrome Scale. A 20-item short form of the LSP-39 (LSP-20) is described which retains 16 items of an earlier short form but which also reproduces the subscale concerned with disability associated with positive psychotic phenomena. The subscales correlated highly with their counterparts in the full form, interrater and test-retest reliabilities were comparable, and concurrent validity was good. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Rosen, Alan and  Trauer, Tom and  Hadzi-Pavlovic, Dusan and  Parker, Gordon},
ISSN = {0004-8674, 1440-1614},
