Year = {1982},
}


@article{1982-03114-00119810301,
Abstract = {Reanalyzed DRL performance in male Sprague-Dawley rats with medial or lateral fornix damage. The reanalysis used multivariate procedures proven effective in analyzing DRL performance in normal rats. The original analysis (C. T. Johnson et al [see PA, Vol 59:2878]) found a deficit after medial fornix damage. The reanalysis of the data revealed more specifically that the deficit following medial fornix lesions was in the Ss' failure to suppress short-latency responses. However, Ss with lateral fornix damage demonstrated a slight deficit in their ability to localize responding to the earliest period in which reinforcement was available. Alternative interpretations are presented in terms of response inhibition and discrimination deficits. (14 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
