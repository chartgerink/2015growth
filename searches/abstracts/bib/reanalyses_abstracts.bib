@article{1979-34737-00119770101,
Abstract = {Hypothesizes that cloze easiness (i.e., the percentage of Ss filling in the correct word in a cloze task) is a measure of information as defined by C. Shannon and W. Weaver (1949). A theory is developed that information is a function of 2 variables: (a) the number of lexical markers associated with the word and (b) the number of lexical markers supplied for the word through what U. Weinrich (1966) calls transfer features. The theory states that frequent words are low information words because they have few lexical markers. Rare words are high information words because they have many markers; however, the literature of automated indexing (e.g., I. Carrol and R. Roeloffs, 1969) shows that rare words that are repeated in a text are closely associated with the topic of the text. It follows that rare words that are repeated in a text are low information words by virtue of extraordinary transfer feature support. The theory is shown to be consistent with a reanalysis of J. Bormuth (1966) data resulting from administering cloze tests to 675 elementary school Ss over 20 passages at 4 levels of difficulty. The theory is examined in light of 3 current models of reading. (French & Spanish abstracts) (19 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Finn, Patrick J.},
ISSN = {0034-0553, 1936-2722},
Journal = {Reading Research Quarterly},
Keywords = {word frequency & meaning & transfer features, cloze performance, elementary school students, Cloze Testing, Grammar, Reading Achievement, Word Frequency, Word Meaning, Elementary School Students},
Number = {4},
Pages = {508 - 537},
Title = {Word frequency, information theory, and cloze performance: A transfer feature theory of processing in reading.},
Volume = {13},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1979-34737-001&site=ehost-live},
Year = {1977},
}


