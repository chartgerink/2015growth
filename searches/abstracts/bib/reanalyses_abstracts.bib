Title = {An illustration of item homogeneity scaling and multilevel analysis techniques in the evaluation of drug prevention programs.},
Volume = {22},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1997-39093-003&site=ehost-live},
Year = {1998},
}


