Year = {1978},
}


@article{1980-08890-00119781101,
Abstract = {Examines the hypothesis that Ss tend to allot figures to the negative poles of constructs approximately 38% of the time, which allows the negative judgments to be maximally striking in their proportion to the whole (the "golden section" is 62–38%). In a replication of a study by J. R. Eiser and C. J. Mower White (see record [rid]1974-09063-001[/rid]), 60 Canadian undergraduates (30 women and 30 men) judged 20 nonsense words (e.g., Johzan) as if these were the names of people on 20 bipolar constructs. 10 of these constructs contained positive poles which were evaluatively positive and negative poles which were evaluatively negative (e.g., kind–not kind) and the other 10 constructs had positive poles which were evaluatively negative and negative poles which were evaluatively positive (e.g., sad–not sad). The results of this experiment and a reanalysis of those of Eiser and Mower White clearly support the hypothesis. (14 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
