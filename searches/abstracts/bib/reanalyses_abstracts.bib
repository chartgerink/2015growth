ISSN = {1357-650X, 1464-0678},
Journal = {Laterality: Asymmetries of Body, Brain and Cognition},
Keywords = {cerebral hemispheres, brain regions interactions, practice, Brain, Interhemispheric Interaction},
Number = {4},
Pages = {361 - 375},
Title = {Practice makes a hemisphere perfect: The advantage of interhemispheric recruitment is eliminated with practice.},
URL = {weissman@duke.edu},
Volume = {8},
Year = {2003},
}


@article{2003-08497-00120030901,
Abstract = {Examined the effect of length variation in the serotonin transporter gene-linked polymorphic region (5-HTTLPR) on individual differences in cardiovascular response to psychological challenge. Heart rate (HR) and systolic and diastolic blood pressure (SBP, DBF) responses to computerized versions of 2 psychological challenges, the Stroop Color-Word Interference Test and mental arithmetic, were measured among 131 monozygotic (MZ) and 60 dizygotic (DZ) male or female (same-sex) European-American twin pairs. Among the 382 Ss, 140 were homozygous for the "long" allele (1/1) at 5-HTTLPR, 61 were homozygous for the "short" allele (s/s), and 181 Ss had one long and one short allele (1/s). Association and sib-pair analyses were performed to characterize genetic associations. In the full sample, 5-HTTLPR was associated with HR reactivity to psychological challenge, albeit in interaction with sex. Task-elicited HR responses of women homozygous for the short allele were significantly greater than among: a) men of the same genotype; and b) women having either one (1/s) or two (1/1) long alleles at 5-HTTLPR. SBP and DBF responsivity was unrelated to genotype. These results were corroborated on reanalysis in 2 genetically independent subsamples. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
