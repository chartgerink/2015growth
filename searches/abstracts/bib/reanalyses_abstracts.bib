Title = {Differences in smoking habits of MZ and DZ twins: A commentary on Tishler and Carey.},
URL = {jaakko.kaprio@helsinki.fi},
Volume = {10},
Year = {2007},
}


@article{2008-06150-00220070901,
Abstract = {Neuroscience research is resulting in an enormous accumulation of complex data. The explosive growth of bioinformatics demonstrated that sharing and reanalysis of digital information can lead to exciting opportunities for scientific integration and discovery. The Society for Neuroscience (SfN) has recently established a Neuroinformatics Committee and launched the Neuroscience Database Gateway, which is already widely used by the neuroscience community. For the 2007 SfN Annual Meeting, we have organized a Satellite Symposium entitled "The Rhyme and the Reason of Data Sharing" to present a series of recent success stories on data sharing and reuse in neuroscience. The neuroscience community is progressively becoming more open to data sharing, and it is hoped that initiatives such as these will help to accelerate and strengthen this process. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
