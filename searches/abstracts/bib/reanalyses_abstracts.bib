Pages = {59 - 70},
Title = {Specialization of neural mechanisms underlying the three-stage model in humor processing: An erp study.},
URL = {chcjyh@gmail.com},
Volume = {32},
Year = {2014},
}


@article{2014-49504-02120141101,
Abstract = {The results of Stolarski, Zajenkowski, and Meisenberg (2013) that were recently published in Intelligence were reanalyzed. Stolarski et al. (2013) found significant associations between the rater-reported nation-level Big Five personality traits agreeableness, extraversion, and openness and intelligence. The present reanalysis reveals that the association between nation-level personality and intelligence is primarily accounted for by the shared variance of the personality traits, also referred to as the General Factor of Personality (GFP), and that the association was not a function of acquiescent responding. Analysis of additional personality data based on self-reports failed to produce meaningful results. (PsycINFO Database Record (c) 2015 APA, all rights reserved). (journal abstract)},
