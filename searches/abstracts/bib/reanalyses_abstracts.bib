@article{1996-03621-00319960401,
Abstract = {This reanalysis of P. Kollock's (see record [rid]1995-13309-001[/rid]) simulations of iterated Prisoner's Dilemmas demonstrate that both the range of noise magnitude and the frequency of noise are important when assessing the success of different strategies in cooperative exchange. When noise is relatively low, as in these simulations, TIT FOR TAT is a very successful overall strategy, while actors using more generous strategies may be exploited by less generous alters. Using lower magnitudes than those used by Kollock, the findings presented here show a complicated picture of mutual invasion by different strategies; no strategy possesses the ability to outperform all others in almost any circumstances. This outcome has a closer fit with real-world situations of social exchange where a variety of strategies can exist. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Reeves, Edward B. and  Pitts, Timothy C.},
ISSN = {0003-1224, 1939-8271},
Journal = {American Sociological Review},
Keywords = {microbased Hobbesian simulation of evolution of social order, macro-outcomes & macrolevel effects on microdecisions in Iterated Prisoners' Dilemma game under uncertainty, commentary, Decision Making, Prisoners Dilemma Game, Simulation, Social Control, Strategies, Social Cognition, Uncertainty},
Number = {2},
