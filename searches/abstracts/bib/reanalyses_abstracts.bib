@article{1987-01090-00119860901,
Abstract = {Suggests that much of the variability in the outcome of recent studies of confidence–accuracy correlations in eyewitness-identification experiments may be accounted for by differences in the design of the research. Specifically, lineup experiments in which the to-be-identified person (target) was always present have tended to produce higher confidence–accuracy correlations than studies also providing blank or target-absent lineups replacing the target with a lookalike. A reanalysis of data reported by the present author and G. L. Wells (see record [rid]1981-32698-001[/rid]) and by the author and H. Wallbridge (1982) revealed that lineup foils (nontargets) were identified with significantly less confidence than targets or looklikes but that targets and looklikes were identified with equal confidence. Two additional experiments tested the hypothesis that confidence of identification is a function of the similarity of the identified person to the original target. Exp I, using 53 university employees and 14 targets to increase generalizability, demonstrated that the person in the lineup who most closely resembled the target was identified with the highest mean level of confidence. Exp II examined confidence in identification following a crime staged for 260 introductory psychology students. Results indicate that the similarity of foils to the target (established by independent ratings) was predictive of both the frequency and confidence of identification of faces. (19 ref) (PsycINFO Database Record (c) 2014 APA, all rights reserved)},
Author = {Lindsay, R. C. L.},
ISSN = {0147-7307, 1573-661X},
