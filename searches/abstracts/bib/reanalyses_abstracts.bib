@article{1985-22587-00119850501,
Abstract = {Reviews the five-fold typology conceptualization of social power developed by J. French and B. H. Raven (1959) and discusses numerous field studies that have used this conceptualization over the past few decades. Unfortunately, a majority of them suffer from severe methodological shortcomings that make their interpretation problematic at best. These problems are discussed, and a reanalysis of the literature is presented. Findings strongly suggest that at least some knowledge about the 5 bases of power (reward, coercive/punishment, legitimate, expert, and referent) is methodologically suspect. Suggestions for improving future research in this domain include attention to the development of more adequate self-report measures, the development of scales to be used as behavioral rather than attributional referents, and concentration on individual- rather than group-level analysis. (82 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Podsakoff, Philip M. and  Schriescheim, Chester A.},
ISSN = {0033-2909, 1939-1455},
Journal = {Psychological Bulletin},
Keywords = {critique of J. French & B. H. Raven's typology of social power, literature review, Literature Review, Social Influences, Interpersonal Control},
Number = {3},
Pages = {387 - 411},
Title = {Field studies of French and Raven's bases of power: Critique, reanalysis, and suggestions for future research.},
Volume = {97},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1985-22587-001&site=ehost-live},
Year = {1985},
}


