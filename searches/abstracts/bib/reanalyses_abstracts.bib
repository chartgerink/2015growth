@article{1988-02830-00119870901,
Abstract = {Marsh and Parker (1984) described the big-fish–little-pond effect ({bflpe}), whereby equally able students have lower academic self-concepts in high-ability schools than in low-ability schools. The present investigation, a reanalysis of the Youth in Transition data, supported the generality of the earlier findings and demonstrated new theoretical implications of the {bflpe}. First, differences in the academic self-concepts of Black and White students, sometimes assumed to represent response biases, were explicable in terms of the {bflpe}. Second, equally able students earned higher grades in lower ability schools. This frame-of-reference effect for grades was distinct from, but contributed to, the {bflpe} for academic self-concept. Third, a longitudinal analysis demonstrated that academic self-concept had a direct effect on subsequent school performance beyond the effects of academic ability and prior school performance. About one quarter of this effect could be explained in terms of the {bflpe}. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Marsh, Herbert W.},
ISSN = {0022-0663, 1939-2176},
Journal = {Journal of Educational Psychology},
Keywords = {high vs low ability academic environment, academic self concept & grades & subsequent school performance, Black vs White males initially assessed in 10th grade, 1 yr longitudinal study, Academic Achievement, Academic Aptitude, High School Students, School Environment, Self Concept, Longitudinal Studies},
