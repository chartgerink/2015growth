Year = {1993},
}


@article{1994-08282-00119930801,
Abstract = {Two new experiments (with a total of 16 male university students) and a reanalysis of R. C. Toye's (see record [rid]1987-27122-001[/rid]) data were used to examine the relationship between true distance and perceived distance in natural scenes. In Exp 1, Ss estimated 78 interobject distances formed by all pairs of 13 objects, while viewing the objects from a fixed position. Results show that estimated distance is a linear function of the visual angle between objects as well as of the true distance. In Exp 2, Ss estimated 6 interobject distances from 12 viewing positions. Results show that visual angle predicted estimated distance independently of how the change in visual angle came about, suggesting that the greater the visual angle between objects, the more their separation is overestimated. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Levin, Charles A. and  Haber, Ralph N.},
ISSN = {0031-5117, 1532-5962},
