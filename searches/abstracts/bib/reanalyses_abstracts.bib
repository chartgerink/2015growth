@article{1998-01835-00719980301,
Abstract = {In an event-related potential (ERP) study using 120 college students, German sentences were investigated that contained a case-ambiguous noun phrase (NP) that could be assigned accusative or dative case. Sentences were disambiguated by the verb in final position of the sentence. Sentences ending in a verb that assigned dative case to the ambiguous NP elicited a clear garden-path effect. The garden-path effect was indicated by a broad centro-posterior negative shift that occurred between 300 and 900 msec after the dative-assigning verb was presented. NPs whose case ambiguity was resolved in favor of accusative case and unambiguously dative-marked NPs did not trigger significant ERP differences. These results support a parser design according to which the so-called structural case is assigned without any delay in the absence of morpho-lexical counterevidence. It is argued that the enhancement of a negative ERP component with a classical N400 topography reflects the difficulty of reanalysis due to reaccessing morpho-lexical information that lies outside the domain of the parsing module. Consequently, ERP responses to garden-path effects are not confined to a late positivity but vary depending on the level of processing involved in reanalysis. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Hopf, Jens-Max and  Bayer, Josef and  Bader, Markus and  Meng, Michael},
ISSN = {0898-929X, 1530-8898},
Journal = {Journal of Cognitive Neuroscience},
Keywords = {ERPs involved in garden-path effect of pure case ambiguities, German speaking college students, Evoked Potentials, Sentence Comprehension, Syntax},
Number = {2},
Pages = {264 - 280},
Title = {Event-related brain potentials and case information in syntactic ambiguities.},
Volume = {10},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1998-01835-007&site=ehost-live},
Year = {1998},
}


@article{1998-01215-00319980201,
Abstract = {Presents a reanalysis of data from an experiment by M. Kubovy and J. Wagemans (see record [rid]1996-92661-001[/rid]) in which they briefly presented multi-stable dot patterns that can be perceptually organized into alternative collections of parallel strips of dots, and in which they parametrically varied the distances between dots and the angles between alternative organizations. The reanalysis shows that relative strength of grouping into strips of dots of a particular orientation approximates a decreasing exponential function of the relative distance between dots in that orientation. The configural or wholistic properties that were varied—such as angular separations of the alternative organizations and the symmetry properties of the dot pattern—do not matter. Additionally, this grouping function is robust under transformations of scale in space (Exp 1) and time (Exp 2). Grouping of units which are themselves the result of a grouping (i.e., pairs of dots; Exp 3) also follows the nonconfigural rule. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Kubovy, Michael and  Holcombe, Alex O.},
ISSN = {0010-0285},
Journal = {Cognitive Psychology},
Keywords = {dot lattice organization, probability of grouping by proximity, adults, reanalysis of  verification of quantitative Gestalt theory, Gestalt Psychology, Spatial Organization, Theory Verification},
Number = {1},
Pages = {71 - 98},
Title = {On the lawfulness of grouping by proximity.},
Volume = {35},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1998-01215-003&site=ehost-live},
Year = {1998},
}


@article{2011-22656-01620111001,
Abstract = {Comments on an article by Taraz Samandari et al. (see record [rid]2011-09737-038[/rid]). The authors conclude that 36 months' isoniazid preventive therapy was more effective than a 6-month course, giving a 74% relative reduction in tuberculosis incidence. It took American Thoracic Society (ATS) a while to come full circle, declaring 6 months to be basically inadequate, after a reanalysis of Alaskan data had suggested that about 9 months were superior to 6 months, but not inferior to 20 months. It thus seems counter intuitive that 36 months were chosen to show the established ineffectiveness of 6 months in HIV infected people rather than 9 or 12 months. The most straightforward conclusion seems that 6 months of preventive therapy for HIV-infected people has never been shown to be appropriate. But the alternative is not 36 months, but rather 9–12 months, as long established. (PsycINFO Database Record (c) 2015 APA, all rights reserved)},
