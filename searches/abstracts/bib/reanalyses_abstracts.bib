Keywords = {worst performance rule, reaction time, IQ, cognitive tasks, diffusion model, Cognitive Processes, Intelligence Quotient, Performance, Reaction Time},
Number = {1},
Pages = {10 - 17},
Title = {A diffusion model explanation of the worst performance rule for reaction time and IQ.},
Volume = {36},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2007-19869-003&site=ehost-live},
Year = {2008},
}


@article{2007-19007-01020080101,
Abstract = {Purpose of review: This review provides an update on use of selective serotonin reuptake inhibitors and youth suicide, and describes an informed examination of the social and professional dimensions of this issue. Recent findings: Recent studies, using various methodologies to analyze experimental and observational data, suggest that concerns about the effect of selective serotonin reuptake inhibitors on suicide and suicide-related phenomena may have been overstated. Also, contrary to much public and medical opinion, treatment of depression with selective serotonin reuptake inhibitors does not increase but rather may decrease youth suicide fatalities. A recent reanalysis by the US Food and Drug Administration of existing data from clinical trials across the lifespan suggest an age-dependent effect on nonfatal suicide attempts and suicidal ideation, in which risk appears to be increased in youth and reduced from mid-adulthood and onward. Summary: Selective serotonin reuptake inhibitors are a modestly effective and generally safe treatment for postpubertal major depressive disorder, but their use requires collaborative decision making and a predetermined, shared monitoring plan. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Kutcher, Stan and  Gardner, David M.},
ISSN = {0951-7367, 1473-6578},
Journal = {Current Opinion in Psychiatry},
Keywords = {selective serotonin reuptake inhibitors, youth suicide, Drug Therapy, Major Depression, Serotonin Reuptake Inhibitors, Side Effects (Drug), Suicide, Reuptake},
Number = {1},
Pages = {65 - 69},
Title = {Use of selective serotonin reuptake inhibitors and youth suicide: Making sense from a confusing story.},
URL = {Stan.Kutcher@dal.ca},
Volume = {21},
Year = {2008},
}


@article{2009-04618-03020090401,
Abstract = {Comments on an article by L. Lecardeur et al. (see record [rid]2009-01129-030[/rid]). The authors explored the relationships between cognitive complaints assessed by the Subjective Scale to Investigate Cognition in Schizophrenia. The authors suggested that patients with schizophrenia might be conscious of their cognitive deficits in spite of a lack of insight concerning their psychotic symptoms. Consequently, in light of our reanalysis we argue that cognitive complaints, cognitive impairments and insight in schizophrenia are three relatively independent phenomenon in schizophrenia. Furthermore, in respect to the two studies discussed here we propose that it would be challenging to explore in a future study the relationships between insight into clinical symptoms and insight into cognitive impairment with objective neuropsychological testing. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
