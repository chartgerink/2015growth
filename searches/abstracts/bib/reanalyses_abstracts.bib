Year = {1982},
}


@article{1983-08108-00119820901,
Abstract = {Previous data indicate that official crime rates were falling in the 2 major industrial countries of the 19th century, Britain and France. Yet sociologists, lawyers, judges, and others continued to discuss "rising crime" and to develop theories to explain why. The reasons for increases differed between schools (biological, psychological, mainstream sociological, and Marxist). But theorists such as E. Durkheim, E. Ferri (1893), J. G. Tarde (1886), F. Engels, and W. A. Bonger (1916) were united in insisting that crime was rising. The present author explores the contradictions between theory and evidence in these various writers. Empirical evidence available at the time is reexamined, along with the results of recent reanalyses of the data. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {McDonald, Lynn},
ISSN = {0007-1315, 1468-4446},
Journal = {British Journal of Sociology},
Keywords = {evidence of vs theorized rising crime in 19th century, Great Britain & France, Crime, History, Theories},
