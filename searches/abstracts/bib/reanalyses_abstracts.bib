@article{1984-16762-00119831001,
Abstract = {Discusses L. G. Rorer's (1965) claim that meaningless acquiescence to attitude and personality scale items does not generalize and hence cannot be a source of artifactual relationships. Rorer excepts ambiguous scales from his own generalizations without providing or suggesting any test to tell which scales are ambiguous to a problematical degree; thus his generalizations are of unknown applicability. A reanalysis of 7 studies by the author (1979–1983) in which balanced scales were scored for acquiescence only revealed many high correlations between such scores, contrary to the findings used by Rorer. Correlations between acquiescence scores derived from balanced attitude scales and balanced personality scales were, however, generally low. It is concluded that meaningless acquiescence may be evinced by respondents to any scale if they find its items ambiguous and that such acquiescence will generalize from scale to scale. Meaningless acquiescence could then create many spurious relationships between unbalanced scales. (41 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Ray, John J.},
ISSN = {0022-4545, 1940-1183},
Journal = {The Journal of Social Psychology},
Keywords = {acquiescent response bias & artifactual relationships in attitude & personality scales, reanalysis of studies by J. J. Ray, Attitude Measures, Personality Measures, Rating Scales, Response Bias},
