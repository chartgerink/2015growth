@article{1986-01002-00119840801,
Abstract = {Conducted an extension of M. Kohn's (1977) work on social class and parental values through a search for value dimensions other than Kohn's self-direction/conformity and an investigation of 3 aspects of social structure: immigrant status, quality of neighborhood, and housing type. Data were collected from parents of 305 9–11 yr olds resident in Sydney, Australia. Kohn's parental values instrument was used, but results were analyzed with a multivariate linear model rather than the traditional way. Immigrant status, neighborhood quality, family income, child's sex, and street type all predicted particular parental value sets. Non-Anglo immigrants differed from other groups in their strong emphasis on school performance, sex-role performance, and sibling relations. Parents in higher quality neighborhoods put greater value on children's sociability than did those in higher risk neighborhoods. Self-control, school performance, and neatness/cleanliness were favored by middle-income families. Parents living in industrial/commercial streets devalued school performance, achievement striving, and curiosity. Parental occupation and education did not have significant predictive power over and above the contribution of these variables. Reanalysis of the data using Kohn's factor analytic procedure gave the traditional self-direction/conformity factor, but immigrant status, quality of neighborhood, and income continued to explain more variance than occupation or education. (45 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Burns, Ailsa and  Homel, Ross and  Goodnow, Jacqueline},
ISSN = {0004-9530, 1742-9536},
Journal = {Australian Journal of Psychology},
Keywords = {immigrant status & neighborhood quality & family income & child's sex & street type, parental values, parents of 9–11 yr olds, Parental Attitudes, Personal Values, Social Environments, Socioeconomic Status},
