Keywords = {role of concordance & complementarity in enactments seen in psychoanalytic treatment, Countertransference, Enactments, Projective Identification, Psychoanalysis, Self Psychology},
Number = {4},
Pages = {706 - 729},
Title = {The role of concordance and complementarity in psychoanalytic treatment.},
Volume = {17},
