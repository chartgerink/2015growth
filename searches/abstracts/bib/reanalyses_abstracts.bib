@article{2000-02021-01420000601,
Abstract = {Describes the technical aspects of 2 arrival-time-differences methods used to locate vocalizing bowhead whales during the spring migration. The results of a field calibration test conducted in situ in 1985 during the bowhead whale migration are provided. The 1985 calibration data and a sample of 217 bowhead calls from 1988 were reanalyzed and used as the basis for comparing and evaluating the reliability of the 2 methods. In the 1st location method (the old method, used in 1984–1986, and 1988) arrival-time difference was estimated by cross correlation of filtered spectrograms. In the 2nd method (the new method, used in 1992–1993), arrival-time difference was estimated by cross correlation of filtered waveforms from pairs of hydrophones. The reanalysis shows that (1) the 2 methods yield similar range distributions, with differences in range estimates increasing as the distance to the vocalizing whale increased, and (2) the range errors associated with location were 4 times smaller and bearing errors were 3 times smaller with the new method. Overall, both methods had approximately the same bias in location estimates. The calibration results show that the new method yielded locations with less variability but only slightly less bias than locations using the old method. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Clark, Christopher W. and  Ellison, William T.},
ISSN = {0001-4966},
Journal = {Journal of the Acoustical Society of America},
Keywords = {calibration & comparison of 2 acoustic location methods during spring migration, bowhead whales, Echolocation, Experimental Methods, Migratory Behavior (Animal), Whales},
Number = {6},
Pages = {3509 - 3517},
Title = {Calibration and comparison of the acoustic location methods used during the spring migration of the bowhead whale, Balaena mysticetus, off Pt. Barrow, Alaska, 1984–1993.},
Volume = {107},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2000-02021-014&site=ehost-live},
Year = {2000},
}


@article{2001-07897-00520000501,
Abstract = {Comments on the article by J. J. Campos et al (see record [rid]2001-07897-001[/rid]) that provides a comprehensive review and reanalysis of the consequences of self-produced locomotor experience. Specifically, the authors focused on the role of locomotor experience in changes in social and emotional development, referential gestural communication, wariness of heights, the perception of self-motion, distance perception, spatial search, and spatial coding strategies. In this commentary, the authors begin by illustrating how the lessons presented by Campos et al can be helpful for the study of temperament. Additionally, the authors take issue with the distinctions raised by Campos et al between experience and biological causes of change. The changes in perceptual abilities and emotional behaviors that Campos et al outline are necessarily the product of brain–behavior interactions. It is possible that the nervous system must be readied for such experiences to have their desired effect. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Marshall, Peter J. and  Fox, Nathan A. and  Henderson, Heather A.},
