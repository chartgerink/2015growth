Journal = {Brain and Language},
Keywords = {model of temporal & neurotopological & neurophysiological structure of syntactic & semantic processes during language comprehension & processing, Comprehension, Language, Semantics, Syntax, Neurophysiology, Neuropsychology},
Number = {3},
Pages = {259 - 281},
Title = {The time course of syntactic activation during language processing: A model based on neuropsychological and neurophysiological data.},
