Volume = {70},
Year = {2013},
}


@article{2005-13027-00820051001,
Abstract = {Response to a reanalysis by Scott D. Slotnick (see record [rid]2005-13027-007[/rid]) of the current author's original study (see reord [rid]2001-16008-001[/rid]). The conclusion drawn by the author in his reanalysis of data from the original study which yielded evidence consistent with a labeled line model of the functional organisation of frontal cortex, can not be supported with the analyses that he describes. This is because his method does not take into account the temporal dependencies of the theoretically discrete processes engaged by the behavioural task that produced the data. That author's analysis may not be appropriate for a reanalysis of the data from Postle et al (2000) is foreshadowed by his statement in the Introduction, that fMRI time courses "are analogous to event-related potential scalp voltage timecourses". This would seem to overlook a critical difference between the two; the latter arises directly from neural activity whereas the former is a lowpass-filtered (i.e., "smeared out") transform of this activity. One consequence of this difference is that simply plucking a time point from an fMRI time course does not yield a "clean," or "process-pure" datum. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Postle, Bradley R.},
ISSN = {0264-3294, 1464-0627},
