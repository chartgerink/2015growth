@article{1999-05929-00519990601,
Abstract = {The occurrence of different conspecific female color morphs, with one of the morphs resembling the male, is supposed to have consequences for mate choice. There are two hypotheses linking mate choice and female color polymorphism. First, males may mate predominantly with female morphs that differ from the male because they do not recognize androchrome females as females (male mimic hypothesis). Second, males may be more attracted to the most common morph in the population (habituation hypothesis). These hypotheses were tested in five populations of the same species, Ischnura elegans, with a range of androchrome frequencies. In each population binary choice experiments were performed in small cages. Males did not consistently prefer gynochrome females but mated predominantly with the most common morph in the population. Moreover, a reanalysis of the available damselfly data in the literature also supported the habituation hypothesis. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Van Gossum, Hans and  Stoks, Robby and  Matthysen, Erik and  Valck, Famke and  De Bruyn, Luc},
ISSN = {0003-3472},
Journal = {Animal Behaviour},
Keywords = {male mimic vs habituation hypotheses of mate choice for female color androchromes vs gynochromes, male damselfly, Animal Coloration, Animal Mate Selection, Hypothesis Testing, Male Animals, Insects},
Number = {6},
Pages = {1229 - 1232},
Title = {Male choice for female colour morphs in Ischnura elegans (Odonata, Coenagrionidae): Testing the hypotheses.},
