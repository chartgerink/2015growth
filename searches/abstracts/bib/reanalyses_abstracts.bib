@article{1994-47314-00119940501,
Abstract = {Reanalyzes the results from the Advertising Research Foundation's (ARF's) Copy Research Validity Project ([CRVP] R. I. Haley and A. L. Baldinger; see record [rid]1991-29222-001[/rid]). The CRVP examined the ability of 35 copy-testing measures to predict TV commercials' sales results, concluding that none could be rejected and implying that advertisement likability is the best single measure. The reanalysis suggests that despite the CRVP's placatory conclusion, a majority of copy-test measures examined in the CRVP clearly can be rejected for general use. Liking's importance has been overstated: Of those measures that survive the decision rule of 90% correct predictions and significantly above-chance prediction at the 10% level of significance, about half involve ad likability but half do not. Moreover, additional tests following the CRVP suggest that liking, too, fails as a general measure and that a better candidate appears to be pre/post persuasion. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Rossiter, John R. and  Eagleson, Geoff},
