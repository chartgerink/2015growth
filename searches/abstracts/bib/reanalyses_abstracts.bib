Year = {2010},
}


@article{2011-12841-00320110701,
Abstract = {Cognitive impairment has been recognized as a significant cause of disability in schizophrenia for more than 20 years, but the development of effective treatments for cognitive impairment in schizophrenia has been slow to advance. The Cognitive Neuroscience Treatment to Improve Cognition in Schizophrenia (CNTRICS) initiative was spearheaded to identify the most important cognitive and affective domains relevant to schizophrenia and to optimize experimental tasks within those domains that possess adequate construct validity. The articles in this issue are the product of a meeting at which issues related to the development and implementation of biomarkers were discussed among experts in various neuroimaging and psychophysiological methods. Collectively, these articles address numerous obstacles that must be overcome to identify usable psychiatric biomarkers. Thus, during this challenging time, projects like CNTRICS provide opportunities to boost the return on scientific investments by extending the shelf life of research data through standardization, integration, sharing, and opportunities for reanalysis. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
