Year = {1997},
}


@article{1997-06223-00119970401,
Abstract = {Presents a reanalysis of 9 positron emission tomography studies involving visual processing to determine the generality of these modulations across a wide variety of visual tasks. Blood flow increases in early visual areas were replicated in 6 of 9 studies, and the test statistic aggregating over studies was highly significant, showing that active tasks can induce modulations in the early stages of the cortical visual pathway. Modulations showed several task dependencies, suggesting that, in some cases, the underlying mechanism was selective rather than nonselective. Replicable blood flow decreases were seen: (1) at or near the auditory Brodmann area left 41/42 in 2 of 5 studies, but in different locations, (2) in the parietal operculum and Brodmann area 40 and, (3) not in the primary somatosensory association area. These results were inconsistent with a model in which the precortical input to task-irrelevant sensory cortical areas was broadly suppressed. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
