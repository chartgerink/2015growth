@article{1974-24273-00119740101,
Abstract = {Examines the 2 most widely accepted definitions of unfair test bias. A. Cleary in 1968 defined unfair test bias in terms of consistent under- or overprediction of actual performance levels of minority or majority groups. R. L. Thorndike's 1971 definition holds that a test is unfairly biased whenever the difference between the minority and majority groups is greater on the test than on actual performance. These 2 definitions, which superficially appear to be similar, are shown to be very different in their implications for minority selection. It is demonstrated that whenever test validity is less than perfect and mean majority criterion performance is higher than that of the minority group, Thorndike's definition leads to acceptance of a larger percentage of minority group members than does the Cleary definition. A review and reanalysis of published research on test bias suggests that most conventional test usages are unbiased by Cleary's definition but unfair by Thorndike's criteria. Advantages and disadvantages of both concepts of unfair test bias are discussed. (32 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Schmidt, Frank L. and  Hunter, John E.},
ISSN = {0003-066X, 1935-990X},
Journal = {American Psychologist},
Keywords = {unfair test bias, implications of T. A. Cleary's & R. L. Thorndike's definitions for minority selection, Cultural Test Bias, Minority Groups, Selection Tests, Test Validity},
Number = {1},
Pages = {1 - 8},
Title = {Racial and ethnic bias in psychological tests: Divergent implications of two definitions of test bias.},
Volume = {29},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1974-24273-001&site=ehost-live},
Year = {1974},
}


@article{1986-03845-00119850301,
Abstract = {In a previous study by the 3rd author et al (see record [rid]1982-32788-001[/rid]), prepubertal depressives were found not to exhibit a shortened REM period latency during active illness when compared with pathological and normal control groups. The present investigation was a reanalysis of REM attempts during the early part of the sleep period of 2 consecutive nights to test the hypothesis that the normal REM period latency during active illness among pubertal depressives in the previous study was due to a nonselective inhibition of REM sleep by slow-wave sleep (SWS). Participants in the 2 studies were 39 Ss with major depression, 19 recovered depressives, 14 Ss with nondepressed emotional disorders, 19 neurotics, and 9 normal Ss (all aged 6–12 yrs). Polysomnographic signs normally associated with REM sleep were observed prior to the 1st REM period; it was thought that these signs might be indicative of minor escapes of REM sleep activity from the inhibitory influence of SWS. Differences among the experimental group in the occurrence (timing and amount per minute) of these signs were hypothesized to support the idea of an inhibitory effect of SWS on REM and thus offer an explanation of the dissociation between the depressive disorder among prepubertal children and a shortened REM period latency. These hypotheses, however, were not supported by the present results, indicating that the dissociation between a shortened REM period latency and depressive disorder among prepubertal children is not due to inhibitory mechanisms of SWS on REM sleep. (20 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Goetz, Raymond R. and  Hanlon, Cleo and  Puig-Antich, Joaquim and  Weitzman, Elliot D.},
ISSN = {0161-8105},
Journal = {Sleep: Journal of Sleep Research & Sleep Medicine},
Keywords = {nonselective inhibition of REM sleep, normal REM period latency, 6–12 yr olds with major depression vs emotional disorders vs neurosis vs Ss who had recovered from depression, Depression (Emotion), REM Sleep},
Number = {1},
Pages = {1 - 10},
Title = {Signs of REM prior to the first REM period in prepubertal children.},
Volume = {8},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1986-03845-001&site=ehost-live},
Year = {1985},
}


@article{1986-03431-00119850901,
Abstract = {Existing research using infra-red corneal reflection techniques to assess infant pattern perception supports the existence of a developmental trend in pattern scanning from narrow to more extensive pattern scanning over the first few months of life; these changes have been attributed to the development of cortical functioning. The present authors discuss the possible sources of error in the use of these techniques with infants and critically evaluate the conclusions that have been drawn from existing studies. In addition to technical limitations in the early corneal reflection work, failure to take account of the infant's state of alertness may have caused younger infants to seem visually less capable than they really are. Based on this review of the existing literature and on a reanalysis of their own (see record [rid]1982-20800-001[/rid]) data on scanning in early infancy, the authors conclude that there are few data that unequivocally support the claimed developmental change in scanning extensiveness in early infancy. (55 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Hainline, Louise and  Lemerise, Elizabeth},
ISSN = {0261-510X, 2044-835X},
Journal = {British Journal of Developmental Psychology},
Keywords = {corneal reflection eye movement recording, measurement of pattern perception, infants, Eye Movements, Pattern Discrimination, Methodology, Perceptual Development},
Number = {3},
Pages = {229 - 242},
Title = {Corneal reflection eye-movement recording as a measure of infant pattern perception: What do we really know?},
Volume = {3},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1986-03431-001&site=ehost-live},
Year = {1985},
}


@article{1986-03423-00119851001,
Abstract = {The authors reanalyzed data from the following 2 studies: (1) the report by J. Perino and the 1st author (see record [rid]1975-09962-001[/rid]) that moderate levels of lead exposure in 80 preschool urban Black children were related to cognitive impairment (the McCarthy Scales of Children's Abilities); and (2) the reexamination by the 1st author and colleagues (1981) of 63 of these children 5 yrs later, which found no significant association between preschool lead level and outcome measures, including cognitive measures, reading tests, and teacher behavior ratings. The positive findings of the preschool study were not substantiated in reanalysis, and the school-age lead data required technical adjustment. Additional variables were generated, including change scores, combinations, and interactions. Of 66 analyses, 2 tests of the lead effect were significant. The first depended on a deviant case; the second was an uninterpretable interaction. It is concluded that if these results are due to a lead effect, the effect is minimal. There was no evidence of an effect of moderate blood lead levels in the preschool period and any school-age outcome measure. (18 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Ernhart, Claire B. and  Landa, Beth and  Wolf, Abraham W.},
ISSN = {0022-2194, 1538-4780},
Journal = {Journal of Learning Disabilities},
Keywords = {lead exposure, cognitive development, Black preschool age children, analysis of data from study by J. Perino & C. Ernhart, Blacks, Cognitive Development, Delayed Development, Lead Poisoning},
