@article{1984-19333-00119840101,
Abstract = {In 3 experiments, 72 male Wistar rats were administered either exogenous insulin or glucose as the UCS paired with a distinctive stimulus complex. Following several such pairings, the administration of a saline (test) solution to the S in that environment produced, in both cases, a reduction in blood-glucose levels (hypoglycemia) as compared to control S who received saline injections throughout. Contrary to previous assumptions, it is suggested that direct changes in blood glucose levels immediately following the administration of either glucose or insulin should not be treated as the UCR. The response of the CNS to the glucose or insulin, and not the immediate effect of increases or decreases in blood glucose levels, must be considered. Reanalysis suggests that in the case of glucose conditioning, the CR produces hypoglycemia which is in the same direction as that of the UCR. In the case of insulin conditioning, the UCR has not been well identified and consequently the CR may be in the same or opposite direction to that of the UCR. (27 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Matysiak, Jan and  Green, Leonard},
ISSN = {0031-9384},
Journal = {Physiology & Behavior},
Keywords = {exogenous insulin or glucose as UCS in classical conditioning paradigms, increase or decrease in blood glucose levels, rats, Blood Sugar, Classical Conditioning, Conditioned Responses, Glucose, Insulin, Hypoglycemia, Rats},
