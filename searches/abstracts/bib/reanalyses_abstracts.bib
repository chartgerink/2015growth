@article{2013-42818-02519850701,
Abstract = {Reply by the current authors to the comments made by Kathi A. Borden et al. (see record [rid]2013-42818-024[/rid]) on the original article (see record [rid]2013-42637-005[/rid]). The argument of Borden et al. rests on two objections-one theoretical, the other methodological. Their theoretical objection is that the measures in the original study are too heterogeneous . They say that only measures of activity should have been used. Their methodological objection is that discriminant function analysis does not work, and should not be used, if the variables being considered are highly intercorrelated: that is, if the variables form a homogeneous set. In their reanalysis Borden et al. use a homogeneous set of measures-those that they think reflect activity level. They then use stepwise discriminant analysis to analyze the resulting data but they pay no attention to the intercorrelation among the ten variables they choose and then report findings that accord with DSM-3. Borden et al. do not succeed in demonstrating that the earlier paper is methodologically flawed. Instead, they contradict themselves. The nature of this self-contradiction, and their failure to note it, is itself instructive. The misrepresentation begins when they purport to have reanalyzed the same data used in the earlier study. Borden et al. also misrepresent the fact that four of the five stepwise discriminant function analyses carried out in the original study were done as variable selection procedures. These four analyses sought to find the variables most highly intercorrelated with, and best representing, the others in the set. This procedure and others, like factor analysis, are often used when there are more variables available for an analysis than can be reliably estimated given the sample size (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Rubinstein, Robert A.},
ISSN = {0002-9432, 1939-0025},
Journal = {American Journal of Orthopsychiatry},
Keywords = {attention deficit disorder, clinical diagnoses, diagnostic category, terminology, Attention Deficit Disorder, Clinical Psychology, Diagnosis},
Number = {3},
Pages = {467 - 469},
Title = {'Validity of attention deficit disorder: A second look': Reply.},
Volume = {55},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2013-42818-025&site=ehost-live},
Year = {1985},
}


@article{2013-21758-01119820601,
Abstract = {Reply by the current author to the comments made by Kenneth Hyltenstam (see record [rid]2013-21758-010[/rid]) on the original article (see record [rid]2013-21754-011[/rid]). According to Hyltenstam’s reply to my reanalysis of his data on the L2 acquisition of negative placement in Swedish, I misrepresent Hyltenstam’s position and my account of the data is neither observationally nor explanatorily adequate. My description of the data apparently misrepresents Hyltenstam because it "does not do justice to.. . the variation that is found in the data," nor is it an adequate account inasmuch as it distinguishes "certain stages" that are "not representative of the data, "whereas on the other hand it “leaves out the initial preverbal phase." Hyltenstam mentions the precedence of the main clause over the subordinate clause structure. This is one of the principles that is also incorporated into my proposal. However, of relevance for the difference between Hyltenstam's and my description is the observation that in the initial stages of language development semantic cues take precedence over morphological markers, whereas morphological markers that are perceptually salient prevail over nonsalient morphological distinctions. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
