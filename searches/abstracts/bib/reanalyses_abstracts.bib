Volume = {102},
Year = {2009},
}


@article{2009-24758-00220090101,
Abstract = {In the recent literature (Adamson 2000; Paradis 2000; Athanasiadou 2007; Traugott 2007a, 2007b), the development of 'emphasizing' meanings by prenominal elements of the English NP is generally envisaged along a path leading from descriptive, prepositional meaning to emphasizing meaning (e.g. from a complete sentence to complete nonsense; cf. Paradis 2000: 245). However, earlier Bolinger (1972: 59, 60) had posited an alternative path for elements that also express determining meanings, i.e. from determining to emphasizing meaning (e.g. from the complete period to a complete foot). This paper will assess these opposed claims about the origin of emphasizing uses by means of a data-based study of the development of two adjectives, particular and specific, that have descriptive, determining, as well as emphasizing uses. Reconstmction of the various meaning extensions and structural reanalyses that have affected the adjectives indicates that the now mostly neglected determining uses are an essential step in the subjectification process to emphasizing meanings and they can shed new light on both the chronology and the conceptual mechanisms involved in the syntactic-semantic shifts. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Ghesquière, Lobke},
ISSN = {0165-4004},
Journal = {Folia Linguistica: Acta Societatis Linguisticae Europaeae},
Keywords = {meanings, adjectives, cognitive semantics, complete sentences, linguistics, Adjectives, Linguistics, Semantics, Sentences},
Number = {2},
Pages = {311 - 343},
Title = {From determining to emphasizing meanings: The adjectives of specificity.},
