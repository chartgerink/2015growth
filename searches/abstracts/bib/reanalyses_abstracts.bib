Title = {On the nature of self-monitoring: Matters of assessment, matters of validity.},
Volume = {51},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1986-26250-001&site=ehost-live},
Year = {1986},
}


