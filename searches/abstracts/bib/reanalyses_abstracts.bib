Title = {Correction and clarification for Ornduff, Freedenfeld, Kelsey, and Critelli (1994); Freedenfeld, Ornduff, and Kelsey (1995); and Ornduff and Kelsey (1996).},
URL = {sornduff@utmem.edu},
Volume = {81},
Year = {2003},
}


@article{2002-06978-00720021101,
Abstract = {This event-related brain potential study assessed the interplay of syntactic and semantic information. The study focused on the N400 component, related to semantic integration, and the SPS/P600 component, related to syntactic reanalysis. 30 German subjects (Ss; mean age 23 yrs) read sentences in which a referent was introduced. Later a personal pronoun was presented which either agreed with the referent in terms of syntactic gender, or in terms of biological gender, or both, or violated both agreements. Results show salient P600 effects for pronouns. This indicates that the establishment of reference involves syntactic reanalysis. N400 effects were observed in sentences with nondiminutives, but not with diminutives. This shows that conceptual/semantic integration is involved during non-diminutive but not during diminutive pronoun processing, or at least it could not be violated. It is concluded that the overall pattern of results supports the claim that for non-diminutives, both syntactic and conceptual information is used to establish co-reference, while for diminutives the process might be purely syntactically driven. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Schmitt, Bernadette M. and  Lamers, Monique and  Münte, Thomas F.},
ISSN = {0926-6410},
Journal = {Cognitive Brain Research},
Keywords = {electrophysiology, biological gender violation, syntactic gender violation, pronouns, German language, Electrophysiology, Language, Pronouns, Semantics, Syntax},
Number = {3},
Pages = {333 - 346},
Title = {Electrophysiological estimates of biological and syntactic gender violation during pronoun processing.},
URL = {b.schmitt@psychology.unimaas.nl},
Volume = {14},
Year = {2002},
}


@article{2002-06730-00320020901,
Abstract = {Notes that forgetting functions generated by delayed matching-to-sample procedures allow delay-dependent effects to be distinguished from delay-independent effects on working memory. Parameters of negative exponential functions estimate initial discriminability (intercept) and rate of forgetting (slope). Forgetting functions for patients with Alzheimer's disease indicate that they differ from normal controls in terms of reduced initial discriminability--that is, in the encoding component of memory performance--but not convincingly in rate of forgetting. Reanalyses of previous studies with different species suggest that pro- and anticholinergic drugs influence initial discriminability in delayed matching-to-sample performance, but not rate of forgetting. The results of the authors' reanalyses are consistent with the conclusion that the cholinergic system plays a role in the encoding component of working memory and that this is the main characteristic of the memory deficit shown by patients with Alzheimer's disease. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {White, K. Geoffrey and  Ruske, Angela C.},
ISSN = {1069-9384},
Journal = {Psychonomic Bulletin & Review},
Keywords = {memory deficits, Alzheimer's disease, encoding hypothesis, cholinergic function, forgetting, initial discriminability, working memory, Alzheimer's Disease, Choline, Forgetting, Human Information Storage, Short Term Memory},
Number = {3},
Pages = {426 - 437},
Title = {Memory deficits in Alzheimer's disease: The encoding hypothesis and cholinergic function.},
URL = {kgwhite@otago.ac.nz, ruske@world-net.co.nz},
Volume = {9},
Year = {2002},
}


@article{2002-17874-00720020601,
Abstract = {In an eye movement experiment, we examined the use of reanalysis strategies during the reading of locally ambiguous but globally unambiguous Spanish sentences. Among other measures, we examined regressive eye movements made while readers were recovering in reading mild garden path sentences. The sentences had an adverbial clause that, depending on the mood (indicative vs. subjunctive) of the subordinate clause verb, could attach high (to the main verb of the sentence) or low (to the verb in the subordinate clause). Although Spanish speakers favor low attachment, the high attachment version was quite easy to understand. Readers predominately used two alternative strategies to recover from the mild garden path in our sentences. In the more common reanalysis strategy, their eyes regressed from the last region (disambiguation+1) directly to the main verb in the sentence. Following this, they reread the rest of the sentence, fixating the next region and the adverb (the beginning of the ambiguous part of the sentence). Less frequently, readers regressed from the last region (disambiguation+1) directly to the adverb. We argue that both types of strategies are consistent with a selective reanalysis process. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Meseguer, Enrique and  Carreiras, Manuel and  Clifton, Charles Jr.},
ISSN = {0090-502X},
Journal = {Memory & Cognition},
Keywords = {reanalysis strategies, garden path sentences, regressive eye movements, adverbial clauses, Spanish speakers, selective reanalysis, Eye Movements, Selective Attention, Sentence Comprehension, Sentence Structure, Strategies},
Number = {4},
Pages = {551 - 561},
Title = {Overt reanalysis strategies and eye movements during the reading of mild garden path sentences.},
URL = {emesegue@ull.es},
Volume = {30},
Year = {2002},
}


@article{2002-01689-01120020601,
Abstract = {The authors (D. Ingram and D. Morehead) comment on their original article (see record [rid]1974-09473-001[/rid]) which found that children with a language impairment do better in the use of inflectional morphology than mean length of utterance (MLU)-matched typically developing children. Other studies have found the opposite relationship. This research note presents a reanalysis of a subset of the original Morehead and Ingram data in an attempt to reconcile these contradictory findings. The reanalysis revealed that the advantage on inflectional morphology for children with language impairment was only on the progressive suffix, not on plural and possessive or on the verbal morphemes third-person present tense and past tense. The resolution of these discrepant results highlights the critical roles that methodological issues play--specifically, how Ss are matched on MLU, how inflectional morphology is measured, and the selection of Ss with regard to age. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Ingram, David and  Morehead, Donald},
ISSN = {1092-4388},
Journal = {Journal of Speech, Language, and Hearing Research},
Keywords = {base syntax development, normal vs linguistically-deviant children, Language Development, Syntax},
Number = {3},
Pages = {559 - 563},
Title = {Morehead & Ingram (1973) revisited.},
URL = {david.ingram@asu.edu},
Volume = {45},
Year = {2002},
}


@article{2002-00576-00620020201,
Abstract = {Although a great deal of research has investigated the factors affecting initial syntactic processing, little is known about the factors that affect processing during reanalysis. To address this question, we report a self-paced reading and an eye-tracking experiment which tested sentences in which there is initially more than one way for reanalysis to proceed, but where this choice is eventually disambiguated by a gender-marked reflexive (like The photographers found the countess who heard the choirboy had really enjoyed himself/herself at the concert in the town hall). Ss were participants and college students in Glasgow. The experiments provide evidence that the human sentence processing mechanism obeys a recency preference in reanalysis. This suggests that at least some of the factors guiding reanalysis are similar to those guiding initial analysis. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Sturt, Patrick and  Scheepers, Christoph and  Pickering, Martin},
ISSN = {0749-596X},
Journal = {Journal of Memory and Language},
Keywords = {sentence processing, ambiguity resolution, reanalysis, recency, syntactic processing, eye-tracking experiments, Sentence Comprehension, Syntax, Experimentation, Eye Movements, Tolerance for Ambiguity},
Number = {2},
Pages = {371 - 390},
Title = {Syntactic resolution after initial misanalysis: The role of recency.},
URL = {sturt@cogsci.ed.ac.uk},
Volume = {46},
Year = {2002},
}


@article{2001-01894-00620010801,
Abstract = {This paper investigates the extent to which existing structural commitments constrain the human parser's search for grammatical analyses of incoming material, specifically whether a Reanalysis As a Last Resort (RALR) strategy applies to sentence parsing. 64 undergraduate students participated in two self-paced reading experiments which investigate this issue using a structural ambiguity in which a local, easy reanalysis is pitted against a nonlocal attachment requiring no reanalysis. This ambiguity is created by embedding classic noun phrase/sentential complement ambiguities inside a relative clause modifying a subject NP. The results of both experiments indicate that readers' existing structural commitments do constrain their subsequent parsing decisions: nonlocal analyses which avoid reanalysis are consistently favored over local analyses which require an easy reanalysis. This conclusion is confirmed by the results of a subcategorization-bias manipulation in Experiment 2, which shows that readers show a consistent bias to avoid reanalysis, rather than a general bias for either local or matrix clause attachments. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Schneider, David and  Philips, Colin},
ISSN = {0749-596X},
Journal = {Journal of Memory and Language},
Keywords = {parsing, ambiguity, reanalysis, monotonic parsing, subcategorization, Cognitive Processes, Discourse Analysis, Phrases, Syntax, Text Structure},
Number = {2},
Pages = {308 - 336},
Title = {Grammatical search and reanalysis.},
URL = {colin@glue.umd.edu},
Volume = {45},
Year = {2001},
}


@article{2001-01894-00520010801,
Abstract = {During language comprehension, people sometimes have to revise their grammatical analysis of a sentence. Experimental evidence demonstrates that such reanalysis often causes processing difficulty. We might therefore predict that reanalysis would be disfavored, with the processor preferring not to reanalyze when it had a choice. Three experiments on complement–clause ambiguities investigate the conditions under which the processor chooses to reanalyze. We contrast two extreme positions, one where the processor avoids reanalysis whenever possible, the other where reanalysis is not disfavored at all. We also consider intermediate positions, in which the preference to maintain the current analysis is one factor that affects ambiguity resolution. The experiments demonstrate that the processor avoids reanalysis even when other factors would support it. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Sturt, Patrick and  Pickering, Martin J. and  Scheepers, Christoph and  Crocker, Matthew W.},
ISSN = {0749-596X},
Journal = {Journal of Memory and Language},
Keywords = {sentence processing, ambiguity resolution, reanalysis, language comprehension, grammatical analysis, Discourse Analysis, Sentence Comprehension, Sentence Structure, Sentences, Syntax},
Number = {2},
Pages = {283 - 307},
Title = {The preservation of structure in language comprehension: Is reanalysis the last resort?},
URL = {sturt@cogsci.ed.ac.uk},
Volume = {45},
Year = {2001},
}


@article{2001-09393-00420010701,
Abstract = {Explores the conclusions about the original internal/external frame of reference (I/E) model--and M. Bong's (see [rid]1998-00166-009[/rid]) attempt to operationalize the internal and external comparison processes that are central to the model and to expand the range of academic self-concept domains--in relation to the measures used in Bong's original research and an appropriate blend of methodology, substantive theory, and the operationalization of theory into method. Bong concluded that the "I/E model failed to receive clear support" (p. 102) in relation to predictions that she derived from her extension of the original model. The authors' critical evaluation and reanalysis, however, reveals problems in the operationalization of the internal and external comparisons, the rationale for post hoc confirmatory factor analysis models and, thus, the original conclusions. The authors' reanalysis, however, provide strong support for the original I/E model, some support for the separation of the internal and external comparison processes, and good support for a new extension to incorporate a wider range of academic domains. After presenting an overview of the methodology of the Bong study, the authors discuss in detail the problems in relation to methods used in the present investigation. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Marsh, Herbert W. and  Yeung, Alexander Seeshing},
ISSN = {0027-3171, 1532-7906},
