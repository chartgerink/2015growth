@article{1997-39164-00119971201,
Abstract = {Asserts, through empirical, numerical, geometric, and algebraic demonstrations, that the positive correlations predicted by C.S. Spearman's (1927) hypothesis (significant mean White-Black IQ differences reflect differences in g, general factor common to all complex tests of mental ability) are psychometric artefacts. It is argued these are artefacts which also arise (1) with measures that have nothing to do with general ability, and (2) with any set of moderately correlated random data, once the sample is split into high and low groups. This interpretation predicts that if sample sizes differ substantially, the correlation will be larger for the 1st principal component of the larger group. This prediction is demonstrated in simulated and real data sets, including toy data from (1969) study of Head Start impact on Black and White children, and A. R. Jensen's (1985) reanalysis of intelligence test data. The author states that positive Spearman hypothesis correlations can be discounted as artefacts under 2 distinct interpretations, and concludes they are a tautological consequence of the facts that IQ test subtests correlate positively, and that Blacks score lower than Whites on these tests. It is concluded they reveal nothing about the nature of g and Black-White differences, and do not warrant any of the extrapolations derived from them, including Jensen's. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Schönemann, Peter H.},
ISSN = {0249-9185},
Journal = {Cahiers de Psychologie Cognitive/Current Psychology of Cognition},
Keywords = {IQ differences as related to g factor loadings & sampling, Blacks vs Whites, critique of C. S. Spearman hypothesis correlations as psychometric artifacts, Intelligence Measures, Psychometrics, Racial and Ethnic Differences, Statistical Samples, Blacks, Factor Structure, Intelligence Quotient, Statistical Correlation, Whites},
Number = {6},
