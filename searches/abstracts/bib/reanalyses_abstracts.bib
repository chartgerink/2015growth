@article{1983-27268-00119830101,
Abstract = {The generalized matching law states that the logarithm of the ratio of responses emitted or time spent responding in concurrent VI schedules is a linear function of the logarithm of the ratio of reinforcements obtained. The slope of this relation, sensitivity to reinforcement, varies about 1.0 but has been shown to be different when obtained in different laboratories. The present paper analyzed the results from 18 experiments on concurrent VI schedule performance and showed that response-allocation sensitivity to reinforcement was significantly smaller when arithmetic, rather than exponential, progressions were used to produce VI schedules. There were no differences in time-allocation sensitivity between the 2 methods of constructing VI schedules. Since the 2 laboratories have consistently used different methods for constructing VI schedules, the differences in obtained sensitivities to reinforcement are explained. The reanalysis suggests that animals may be sensitive to differences in the distribution of reinforcements in time. (34 ref) (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Taylor, Russell and  Davison, Michael},
ISSN = {0022-5002, 1938-3711},
Journal = {Journal of the Experimental Analysis of Behavior},
Keywords = {arithmetic & exponential schedules, response allocation sensitivity to reinforcement, literature review, Literature Review, Reinforcement Schedules, Variable Interval Reinforcement},
Number = {1},
