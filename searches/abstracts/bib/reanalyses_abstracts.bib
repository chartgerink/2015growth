Title = {Crossing Form and Function: First and Second Person Plural Imperatives in the Dialect of Mesocco.},
URL = {loporcar@rom.unizh.ch},
Volume = {40},
Year = {2006},
}


@article{2013-02135-00920130201,
Abstract = {Presents a case report of Auguste Deter, a middle aged woman in whom Alois Alzheimer was first reported. The patient came to Alzheimer’s attention when she was admitted to his psychiatric service at age 51 years, because of substantial forgetfulness accompanied by hallucinations. Dementia progressed rapidly and she died 5 years later, in 1906, at age 56 years. Neuropathological analysis of her cerebral cortex revealed many amyloid plaques and neurofibrillary tangles. Originally, the term Alzheimer’s disease referred to presenile dementia as reported by Alzheimer in Auguste Deter and in another patient. Both the patient records of Auguste Deter and the histological slides prepared from her brain in Alzheimer’s laboratory have been rediscovered. Neuropathological reanalysis confirmed the original diagnosis. Because mutations in PSEN1 are the most common cause of autosomal dominant Alzheimer’s disease, the authors analyzed PSEN1 in DNA extracted from a histological section of Auguste Deter’s brain. In summary, the PSEN1 mutation in Auguste Deter is consistent with the clinical findings and uncovers the cause of disease in this important historical case. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Müller, Ulrich and  Winter, Pia and  Graeber, Manuel B.},
ISSN = {1474-4422},
