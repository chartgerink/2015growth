Title = {Size estimation in schizophrenics: A review and reanalysis.},
Volume = {71},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1969-08513-001&site=ehost-live},
Year = {1969},
}


@article{1969-07555-00119690201,
Abstract = {Smallest space analysis is more parsimonious than factor analysis in the sense that it usually renders a space of fewer dimensions. Interpretation of such a space is facilitated by an a priori definitional system for the variables being studied. A reanalysis of the intercorrelation matrix in a previous study by D. J. Hoger by means of the Guttman-Lingoes SSA-I computer program shows how a certain definitional structure of the test variables is reflected in a 2-space. Contiguous regions in this space represent the task of the S rule-inferring, rule-applying, and school achievement and the language of presentation of the item: verbal, digital, and figural. The 6 orthogonal common factors extracted by Hoger's factor analysis can be represented as 6 points in a 2-space simultaneously with the tests. Results add further confirmation to the utility of the faceted definition of intelligence and the radex hypothesis for the structure of test interrelationships. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Schlesinger, I. M. and  Guttman, Louis},
ISSN = {0033-2909, 1939-1455},
Journal = {Psychological Bulletin},
Keywords = {smallest space analysis of intelligence & achievement tests, Achievement Measures, Computers, Intelligence Measures, Mathematics (Concepts), Statistical Analysis},
Number = {2},
