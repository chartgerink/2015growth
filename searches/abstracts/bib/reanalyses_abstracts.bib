Title = {Distorting judgments of mental health: Generality of the labeling bias effect.},
Volume = {161},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1976-02480-001&site=ehost-live},
Year = {1975},
}


