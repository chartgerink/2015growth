URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2000-15125-006&site=ehost-live},
Year = {2000},
}


@article{2000-03123-00320000101,
Abstract = {In this paper the authors discuss an asymmetry in the Case system of German and its implications for human sentence processing: the asymmetry between nominative/accusative and dative case. Starting from the assumption that dative case has a distinct grammatical representation, the results of two experiments (n = 64) will be presented, which show that dative assignment during reanalysis is accompanied by additional processing operations that are not needed when accusative or nominative are assigned. In particular, the authors show that dative assignment during reanalysis triggers reaccess to the mental lexicon, giving rise to greater processing difficulty. The authors conclude with a discussion of empirical and theoretical consequences of our findings. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
