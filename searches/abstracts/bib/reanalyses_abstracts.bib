Title = {The Chemnitz LogAnalyzer: A tool for analyzing data from hypertext navigation research.},
URL = {angela.brunstein@phil.tu-chemnitz.de},
Volume = {37},
Year = {2005},
}


@article{2005-06788-00220050501,
Abstract = {A model for systematic changes in patterns of inter-individual variation in affective responses to physical activity of varying intensities is presented, as a conceptual alternative to the search for a global dose-response curve. It is theorized that trends towards universality will emerge in response to activities that are either generally adaptive, such as moderate walking, or generally maladaptive, such as strenuous running that requires anaerobic metabolism and precludes the maintenance of a physiological steady state. At the former intensity the dominant response will be pleasure, whereas at the latter intensity the dominant response will be displeasure. In contrast, affective responses will be highly variable, involving pleasure or displeasure, when the intensity of physical activity approximates the transition from aerobic to anaerobic metabolism, since activity performed at this intensity entails a trade-off between benefits and risks. Preliminary evidence in support of this model is presented, based on a reanalysis of data from a series of studies. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Ekkekakis, Panteleimon and  Hall, Eric E. and  Petruzzello, Steven J.},
ISSN = {0264-0414, 1466-447X},
Journal = {Journal of Sports Sciences},
Keywords = {affective responses, physical activity, inter individual variation pattern, adaptive activities, evolutionary dose response model, Emotional Responses, Evolutionary Psychology, Exercise, Motor Processes, Response Variability, Activity Level, Health Promotion, Physical Activity},
Number = {5},
Pages = {477 - 500},
Title = {Variation and homogeneity in affective responses to physical activity of varying intensities: An alternative perspective on dose-response based on evolutionary considerations.},
URL = {ekkekaki@iastate.edu},
Volume = {23},
Year = {2005},
}


