Title = {Variables affecting naming latency for Japanese Kanji: A reanalysis of Yamazaki, et al (1997): Erratum.},
URL = {n.shibahara@ucl.ac.uk},
Volume = {96},
Year = {2003},
}


@article{2003-03062-00120030201,
Abstract = {This reanalysis of a previously reported clinical trial assessed whether quetiapine or haloperidol has benefits for the treatment of hostility and agitation among patients experiencing an acute exacerbation of schizophrenia. 257 Ss aged 18–65 yrs of either sex and any ethnicity who had a diagnosis of schizophrenia and who were experiencing an acute exacerbation were recruited. A priori, data from patients assigned to 4 therapeutically effective quetiapine treatment groups (150, 300, 600, and 750 mg) in a previously reported 6-week, double-blind, placebo-controlled clinical trial were combined and compared with data from patients given haloperidol 12 mg or placebo on an agitation measure derived from the Brief Psychiatric Rating Scale (BPRS). Quetiapine treatment reduced agitation scores significantly among patients with acute psychoses. A slight reduction in agitation scores was found when haloperidol treatment was compared with placebo. Compared with haloperidol, quetiapine treatment had a direct and significant effect on agitation that was independent of the improvement in psychotic symptoms. The data suggest that quetiapine treatment has benefits for hostility and agitation among patients experiencing an acute exacerbation of schizophrenia. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Chengappa, K. N. Roy and  Goldstein, Jeffrey M. and  Greenwood, Mike and  John, Vineeth and  Levine, Joseph},
ISSN = {0149-2918},
