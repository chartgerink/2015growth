@article{2004-12618-00520031201,
Abstract = {Coming of age is the biopsychosocial process by which the ascending generation achieves autonomy, independence, and self-responsibility. In the psychic realm these have a parricidal significance and trigger conflicts of aggression. The analysis of these conflicts poses special problems of methodology and technique for the analyst who needs to make the analytic situation a safe place for himself when he finds himself on the receiving end of the patient's aggression. Three men who came to analysis with late adolescent-young adult issues are presented. Two were also in analysis in childhood and the third returned for analysis in late middle age. The need for reanalysis is examined in the framework of contemporary concepts of development. Other motivations for returning to analysis are identified: Analysis in childhood tends to establish the analyst as guide and mentor paralleling the parent-child relationship; the analytic experience fosters the conviction that understanding how the mind works is an essential tool for living more comfortably with oneself and others. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Ritvo, Samuel},
ISSN = {1076-044X},
Journal = {Journal of Clinical Psychoanalysis},
Keywords = {coming of age, biopsychosocial process, aggression, psychoanalysis, reanalysis, development, Aggressiveness, Biopsychosocial Approach, Development, Psychoanalysis},
Number = {1},
Pages = {31 - 54},
Title = {Conflicts of Aggression in Coming of Age: Developmental and Analytic Considerations; Observations on Reanalysis.},
Volume = {12},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2004-12618-005&site=ehost-live},
Year = {2003},
}


@article{2004-12409-00220031201,
Abstract = {This article examines principals' espoused values and their values in action. It provides a reanalysis of previously collected data through a values lens. The original research study was an international quantitative and qualitative investigation of principals' leadership approaches that was based in 15 schools. This particular excerpt of the research provides a qualitative lens into two European principals' espoused values versus values in action in a site-based management environment. The study demonstrates that principals' espoused values can be different from their values in action as perceived by themselves and others. This has implications for the practical application of modern school restructuring site-based management theory, and it shows the need for a value-based component to principals' leadership training. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
