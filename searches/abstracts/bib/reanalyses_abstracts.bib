Number = {3},
Pages = {415 - 424},
Title = {Heart rate reactivity in male batterers: A second look at the evidence.},
Volume = {15},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2001-11319-005&site=ehost-live},
