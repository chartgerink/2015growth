Pages = {146 - 157},
Title = {Ethical dilemmas, good intentions, and the road to hell: A clinical-ethical perspective on Yalom's depiction of Trotter's therapy.},
URL = {rubin@psy.haifa.ac.il},
Volume = {64},
Year = {2001},
}


