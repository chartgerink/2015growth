@article{1978-00288-00119770201,
Abstract = {In most current accounts of character classification, emphasis is laid on the role of the positive set. Reanalysis of a 1970 experiment by the present author showed that in the constant set procedure, as well as the varied set procedure, when consecutive sets of digits are used, latency to negative probes depends on remoteness from the positive set boundary, except when the positive set consists of only 1 item. Two further experiments with 30 undergraduates showed that in both the constant- and varied-set procedures, latency to individual negative probes decreased with their frequency, except with positive sets of 1 item. These data indicate that negatives are not usually classified as such merely by default. Some alternative models to S. Sternberg's (1966, 1967, 1970) are discussed. It is proposed that when an S is able to (always with 1-item positive sets) he/she will perform a physical match. Otherwise, the probe will access its location in long-term memory, associated with which is appropriate response information. The ease of recovery of that information is emphasized as responsible for variations in latencies. (46 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Marcel, Anthony J.},
ISSN = {0033-555X},
Journal = {The Quarterly Journal of Experimental Psychology},
Keywords = {probe frequency, negative set effects in character classification employing constant & varied set procedures, college students, interpretation based on response-retrieval view of RT, Cognitive Processes, Memory, Reaction Time, Stimulus Frequency, Stimulus Presentation Methods},
