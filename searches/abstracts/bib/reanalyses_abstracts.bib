Title = {Information processing through the first year of life: A longitudinal study using the Visual Expectation Paradigm.},
Volume = {62},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1997-43435-001&site=ehost-live},
Year = {1997},
}


@article{1997-42484-00219970101,
Abstract = {Previous work has shown that sentences with actional verbs (e.g., to hit) are better understood by children than sentences with nonactional verbs (e.g., to see). The distinction between the class-based and verb-based explanations of the actionality effect is discussed, and the validity of their empirical basis is questioned. The actionality effect is accounted for in contrasted ways in the literature. These explanations are analyzed and found wanting both theoretically and empirically. An alternative suggestion is made. It is based on new data as well as on a reanalysis of previously published ones. Accordingly, actional verbs favor the construction of mental images that serve as supports for the operations involved in processing sentences. (PsycINFO Database Record (c) 2014 APA, all rights reserved)},
