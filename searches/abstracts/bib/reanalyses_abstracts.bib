Title = {An estimate of variance due to traits in leadership.},
Volume = {68},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1984-03977-001&site=ehost-live},
Year = {1983},
}


@article{1984-03753-00119830401,
Abstract = {The recognition of the lateralization of language to the left hemisphere in most adults was a triumph of 19th-century neurology, but because the clinical finding of frequent crossed aphasia in children suggested that language functions were bilateral during childhood, the notion arose that lateralization was a developmental process that occurred over the 1st decade of life. Recent neuroanatomical, EEG, and neuropsychological investigations have tended to support the contrary view that language is primarily lateralized to the left hemisphere from the 1st mo of postuterine life. Now the collection of more clinical data on acquired childhood aphasia and the reanalysis of earlier clinical studies indicate that the modern incidence of crossed aphasia in children is too low to support the hypothesis of a gradual lateralization of language over time and is more consistent with a theory of developmental invariance of language lateralization. (20 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Woods, B. T.},
ISSN = {0166-2236},
Journal = {Trends in Neurosciences},
Keywords = {theory of developmental invariance of language, Cerebral Dominance, Development, Language, Theories},
Number = {4},
