Keywords = {independence assumption, simultaneous base-rate & payoff, decision criterion learning, discriminabilities, perceptual categorization, flat-maxima, competition between reward & accuracy maximization, Classification (Cognitive Process), Decision Making, Learning, Visual Discrimination, Competition, Rewards},
Number = {2},
