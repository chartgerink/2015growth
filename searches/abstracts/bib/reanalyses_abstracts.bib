@article{2008-04662-02020080401,
Abstract = {Comments on an article by M. Catherine DeSoto and Robert T. Hitlan (see record [rid]2007-18726-004[/rid]). DeSoto and Hitlan reanalyzed an original data set, concluding that a relationship exists between blood mercury levels and the diagnosis of autism spectrum disorder (ASD). The conclusion is based on the reanalysis of hair to blood mercury ratios. As elegantly demonstrated by Grandjean and colleagues, mercury levels in the hair reflect a delayed average compared to the blood mercury level averages. That is, mercury hair concentrations at hypothetical time point T reflect blood mercury levels at T minus 1 to 2 months. The analysis by DeSoto and Hitlan, which presumes that the 2 biomarkers are equally affected, is clearly erroneous. Thus, absent appropriate corrections for the temporal fluctuations in mercury levels, the conclusions should be interpreted with utmost caution and revalidated taking the above issue into account. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Aschner, Michael},
ISSN = {0883-0738, 1708-8283},
Journal = {Journal of Child Neurology},
Keywords = {mercury, diagnosis, blood levels of mercury, autism, autism spectrum disorder, Autism, Blood, Diagnosis, Mercury (Metal), Pervasive Developmental Disorders},
