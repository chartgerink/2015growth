Year = {1996},
}


@article{1996-03338-00519960301,
Abstract = {Examined learner variables that are related to mathematics achievement in actual learning situations. The dynamic model of the learning process as developed by Boekaerts (e.g., 1992) was taken as a starting point. Both traitlike self-referenced cognitions (i.e., academic self-concept of mathematics ability, goal orientation, attribution style) and situation-specific variables were included. Reanalyses were conducted on previous data (Seegers and Boekaerts, 1993) from 90 male and 96 female 11–12 yr old primary school students. Marked differences were found between boys and girls on a mathematics test. These differences were parallelled by differences in both traitlike self-referenced cognitions and task-specific appraisals. It is concluded that boys experience learning situations where they are confronted with a mathematics test in a more positive way than girls do. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
