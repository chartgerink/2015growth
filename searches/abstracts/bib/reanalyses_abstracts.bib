Year = {1974},
}


@article{1974-28881-00119740301,
Abstract = {Suggests that attempts to develop physical theories of psi are inadequate. Precognition is the most difficult type of psi to explain and appears to require a comprehensive reanalysis of its characteristics. A form of "psi physics" is needed to explore psi interaction, and "psi biology" is needed for the comparative study of psi in different species, focusing on the larger question of evolution. It is concluded that parapsychical issues are vital to humanity, but until the problems of psi physics and biology are defined and solved, the question of human survival cannot be answered by science. (4 p ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
