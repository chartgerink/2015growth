@article{1997-38744-00219980101,
Abstract = {The role of semantic transparency in morphological and in compound processing is examined. It is argued that the notion of semantic transparency is crucial to an account of how compounds are represented and processed in the mind. A sketch of a model is proposed in which compound processing is described in terms of stimulus, lexical, and conceptual, properties. The model represents the notion of semantic transparency in terms of a four-way classification of the semantic relationship between a compound's constituents and the corresponding independent morphemes. It also distinguishes between semantically componential and noncomponential compounds. It is proposed that the model offers a framework within which experimental psycholinguistic findings can be understood and within which aphasic deficits associated with compound processing can be characterized. The paper presents a reanalysis of an aphasic patient who exhibits the tendency to interpret semantically opaque compounds as though they were transparent and to interpret opaque compounds in terms of a blend of constituent and whole-word meaning. It is argued that the underlying deficit in this patient is the failure for inhibition to result from the competition among stimuli at the conceptual level of representation. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Libben, Gary},
ISSN = {0093-934X},
Journal = {Brain and Language},
Keywords = {semantic transparency in morphological & compound processing & consequences for representation & impairment, Morphology (Language), Psycholinguistics, Semantics},
Number = {1},
Pages = {30 - 44},
Title = {Semantic transparency in the processing of compounds: Consequence for representation, processing, and impairment.},
Volume = {61},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1997-38744-002&site=ehost-live},
Year = {1998},
}


@article{1997-38522-00119980101,
Abstract = {Reports on the development, validation, and application of the Adaptation to Age-Related Vision Loss (AVL) Scale, a measure of psychosocial adaptation to visual impairment specifically designed for older persons. This adaptation encompassed acceptance of vision loss, attitudes toward rehabilitative training, and attitudes toward relationships with others. After a preliminary 33-item AVL Scale was developed, it was administered to 2 samples totaling 498 65–100 yr olds. 48% of Ss in the 1st study, and 39.2% in the 2nd were legally blind. Results of item analyses of the 33-item scale, factor structure and internal consistency analyses of the final 24-item AVL Scale, reanalysis of data from both studies, and validity analysis, support the reliability and validity of the AVL Scale. Uses of the AVL Scale are described, including survey research, clinical assessments, and evaluation of vision rehabilitation interventions. Limitations include use of a dichotomous scoring system which may have contributed to failure to identify independent factors, and ceiling effects that may limit utility in assessing change over time. Preliminary results from its application have provided empirical support for the benefit of vision rehabilitation services. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Horowitz, Amy and  Reinardt, Joann P.},
ISSN = {0145-482X},
