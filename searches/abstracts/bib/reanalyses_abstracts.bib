Year = {1992},
}


@article{1993-06487-00119920101,
Abstract = {Analyses of data from the Menninger Psychotherapy Research Project (MPRP) have consistently indicated little difference in therapeutic outcome between 22 outpatients seen in psychoanalysis (PSA) and 20 outpatients seen in psychotherapy (PT). A reanalysis of the data from the MPRP using a distinction between 2 broad configurations of psychopathology indicates that anaclitic patients, whose pathology focuses on disruptions of interpersonal relatedness and who use primarily avoidant defenses, have significantly greater positive change in PT. Introjective patients, whose pathology focuses on issues of self-definition, autonomy, and self-worth and who use mainly counteractive defenses, have significantly greater positive change in PSA. The clinical implications of patient-by-treatment interactions are discussed, and the importance of differentiating among types of patients in studies of therapeutic process and outcome is stressed. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
