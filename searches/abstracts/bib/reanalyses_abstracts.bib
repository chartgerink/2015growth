@article{1995-28010-00119950301,
Abstract = {This article focuses on decision processes in recognition memory. It begins with investigation of the hypothesis that the measured criterion increases systematically with the memorability of old items. Three experiments using the list-strength paradigm, and a review of the prior literature, present results consistent with this hypothesis. Several psychological models of criterion placement are examined, generating different predictions about the relative sizes of criterion shifts for strong and weak items. A range model, in which criterion placement depends on the estimated range of the old and new distributions, predicts that criterion shifts should be larger for weak items; this result emerges in a reanalysis of prior studies. The general discussion elaborates on how a focus on criterion placement can explain the mirror effect (Glanzer, Adams, Iverson, & Kim, see record [rid]1993-40158-001[/rid]) and provides a framework for testing Shiffrin, Ratcliff, and Clark's (see record [rid]1990-13917-001[/rid]) claims about why null effects of list strength occur with repetition. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Hirshman, Elliot},
ISSN = {0278-7393, 1939-1285},
Journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
Keywords = {stimulus memorability, effect of criterion shifts on recognition memory in list strength paradigm, college students, Decision Making, Memory, Recognition (Learning), Stimulus Salience},
Number = {2},
Pages = {302 - 313},
Title = {Decision processes in recognition memory: Criterion shifts and the list-strength paradigm.},
Volume = {21},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1995-28010-001&site=ehost-live},
Year = {1995},
}


@article{1995-24589-00119950301,
Abstract = {Examined whether the experience of being cared for by a depressed mother stunts an infant's intellectual development, using prospective longitudinal data that had previously been obtained from a community sample of 119 primiparous North London women (age at entry 28 yrs), followed from early pregnancy until the children were 4 yrs old. Reanalysis of those data provided support for the original finding of an association between postnatal depression and impaired cognitive abilities in the children. Children whose mothers had been depressed showed reliably lower scores on the McCarthy Scales of Children's Abilities at 4 yrs of age, with the perceptual performance scale most affected. The decrements were not accounted for by prenatal and perinatal experiences nor by the family's present status and climate. However, low birth weight appeared to potentiate the effect, whereas higher levels of maternal education served as a protective factor. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
