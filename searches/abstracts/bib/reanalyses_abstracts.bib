Number = {3-4},
Pages = {235 - 243},
Title = {Gender differences in antidepressant and activating drug effects on self-perceptions.},
Volume = {7},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1986-04493-001&site=ehost-live},
