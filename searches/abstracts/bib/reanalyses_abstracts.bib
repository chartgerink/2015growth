ISSN = {1094-4281, 1552-7425},
Journal = {Organizational Research Methods},
Keywords = {ethnostatistics, medical innovation, structural equivalence, network data reanalyses, new variables, private practice physicians, social network approach, judgment calls, psychophysics power function, Creativity, Physicians, Psychophysics, Social Networks, Statistical Analysis, Ethnography, Innovation, Judgment, Private Practice, Statistical Variables},
