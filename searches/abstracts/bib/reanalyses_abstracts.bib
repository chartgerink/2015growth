Year = {2000},
}


@article{2000-15125-01120000301,
Abstract = {When the human parser encounters a local structural ambiguity, are multiple structures pursued (parallel or breadth-first parsing), or just a single preferred structure (serial or depth-first parsing)? This note discusses four important classes of serial and parallel models: simple limited parallel, ranked limited parallel, deterministic serial with reanalysis, and probabilistic serial with reanalysis. It is argued that existing evidence is compatible only with probabilistic serial-reanalysis models, or ranked parallel models augmented with a reanalysis component. A new class of linguistic structures is introduced on which the behavior of serial and parallel parsers diverge the most radically: multiple local ambiguities are stacked to increase the number of viable alternatives in the ambiguous region from two to eight structures. This paradigm may provide the strongest test yet for parallel models. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Lewis, Richard L.},
ISSN = {0090-6905, 1573-6555},
Journal = {Journal of Psycholinguistic Research},
Keywords = {serial vs parallel models of parsing structural ambiguity in sentence processing, Contextual Associations, Sentence Comprehension, Syntax, Verbal Meaning},
Number = {2},
Pages = {241 - 248},
Title = {Falsifying serial and parallel parsing models: Empirical conundrums and an overlooked paradigm.},
Volume = {29},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2000-15125-011&site=ehost-live},
Year = {2000},
}


@article{2000-15125-00620000301,
Abstract = {Discusses the nature of syntactic reanalysis in sentence processing, paying particular attention to questions about whether and how it differs from the construction of an initial analysis, such as whether it is encapsulated or whether alternatives are considered in parallel or not. The authors outline different strategies that the processor may use in reanalysis, and review the relevant evidence. The experimental evidence reviewed is compatible with an encapsulated serial search strategy. Within the restricted search space of the parser, minimal-revisions behavior is obtained by ordering less-destructive parsing operations before more destructive operations. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Sturt, Patrick and  Pickering, Martin J. and  Crocker, Matthew W.},
