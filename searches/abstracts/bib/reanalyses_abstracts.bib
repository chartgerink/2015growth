@article{1989-72223-00119870101,
Abstract = {Discusses the significance of Freud's essay "Die endliche und die unendliche Analyse" ("Analysis terminable and interminable") (1937) from a historical perspective. It is argued that in this essay, Freud developed several themes of profound importance to psychoanalysis and, at the same time, reactivated some antiquated ideas (e.g., the concept of ego instinct). Freud's observations on the nature of termination and on incomplete, complete, periodic, and interminable analysis are reviewed; his recommendations for reanalysis and ongoing self-analysis of psychoanalysts are noted. (English abstract) (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Blum, Harold P.},
