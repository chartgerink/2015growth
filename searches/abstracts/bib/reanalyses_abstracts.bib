Year = {1978},
}


@article{1979-00384-00119780101,
Abstract = {Four White Carneaux pigeons chose between FI schedules of different durations presented in the terminal links of concurrent-chains schedules. The pair of schedules was always in the ratio of 2:1, but the absolute duration of the FIs varied. In 1 set of conditions, the different terminal-link schedules were associated with different keylight stimuli (cued conditions). In a 2nd set, the different terminal-link schedules were associated with the same stimulus (uncued conditions). Results from the cued conditions replicated previous findings that preference for the shorter FI schedule increased with FI duration. Preferences in the uncued conditions were lower than in the corresponding cued conditions but also increased with FI length. Degree of control under the uncued conditions was correlated with the extent to which the schedule during the terminal link was discriminated immediately on entry into the terminal link. The pattern of results in both conditions is inconsistent with the notion that choice behavior matches relative immediacy of reinforcement. Reanalysis of S. H Chung and R. J. Herrnstein's (see record [rid]1967-05743-001[/rid]) evidence for matching showed that matching in fact did not occur, as the preferences of their Ss for the shorter of 2 delays also increased with the absolute size of the delays. (20 ref) (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Williams, Ben A. and  Fantino, Edmund},
ISSN = {0022-5002, 1938-3711},
Journal = {Journal of the Experimental Analysis of Behavior},
Keywords = {reinforcement delay & conditioned reinforcement, choice, pigeons, Animal Motivation, Choice Behavior, Fixed Interval Reinforcement, Reinforcement, Reinforcement Schedules, Pigeons},
Number = {1},
Pages = {77 - 86},
Title = {Effects on choice of reinforcement delay and conditioned reinforcement.},
Volume = {29},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1979-00384-001&site=ehost-live},
Year = {1978},
}


