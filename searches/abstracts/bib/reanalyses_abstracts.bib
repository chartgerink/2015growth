Number = {1},
Pages = {36 - 45},
Title = {Augmentation of fluoxetine's antidepressant action by pindolol: Analysis of clinical, pharmacokinetic, and methodologic factors.},
Volume = {21},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2001-14228-006&site=ehost-live},
Year = {2001},
}


