Keywords = {anthropology, Pidgin & Creole studies, Creole languages, cognitive processes, Anthropology, Cognitive Processes, Language, Racial and Ethnic Groups, Dialect},
Pages = {33 - 42},
Title = {New Directions in Pidgin and Creole Studies.},
