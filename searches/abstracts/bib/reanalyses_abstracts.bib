Keywords = {sequencing of change processes by stage across health problems & transtheoretical model & health promotion, Behavior Change, Disorders, Health Promotion, Lifestyle Changes, Models},
Number = {6},
Pages = {593 - 604},
Title = {Is the sequencing of change processes by stage consistent across health problems? A meta-analysis.},
Volume = {19},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2000-16348-012&site=ehost-live},
Year = {2000},
}


@article{2000-16182-00120001101,
Abstract = {Performed a qualitative and quantitative reanalysis of the Six Cultures data on children's play, collected in the 1950s, to revisit worlds of childhood during a time when sample communities were more isolated from mass markets and media than they are today. A count was performed of 140 children (aged 3–10 yrs) in each community sample scored as engaging in creative-constructive play, fantasy play, role play, and games with rules. Results showed that children from Kenya and India scored lowest overall, those from the Philippines and Mexico scored intermediate, and those from Japan and the US scored highest. It is concluded that cultural norms and opportunities—or whether adults encouraged work versus play, children had freedom for exploration and motivation to practice adult roles through play, and the environment provided easy access to models and materials for creative and constructive play—determined how the kinds of play were stimulated. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
