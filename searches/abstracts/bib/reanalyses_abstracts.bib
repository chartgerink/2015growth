@article{2001-09622-01220010101,
Abstract = {Presents data supporting a (possibly specific) antisuicidal effect of lithium (LI). The IGSLI study (International Group for the Study of Lithium-treated Patients) covering 5,616 patient years clearly showed that adequate long-term LI treatment significantly reduces and even normalizes the excess mortality of patients with affective disorders (ADs). A metaanalysis on 17,000 Ss pooled from 28 studies showed that the rate of suicidal acts is 8.6 fold higher in Ss without LI compared to those with regular LI treatment. A post-hoc analysis of a large multicenter, controlled long-term trial found no suicidal acts in 146 Ss randomized to LI compared to 9 suicidal acts in 139 Ss randomized to carbamazepine. Reanalysis of the data from the IGSLI study supports the concept of the specificity of LI, i.e., evidence could be provided that LI also reduces suicidal behavior in patients who do not benefit from the LI treatment in terms of episode reduction. LI has to be considered as a first line mood stabilizer in ADs, particularly in patients with a history of suicide attempts. Extreme caution is required when LI is discontinued or a patient is switched to another mood stabilizer, because such a patient might have been protected against suicidal impulses in spite of an incomplete response. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Müller-Oerlinghausen, B.},
ISSN = {0940-1334, 1433-8491},
Journal = {European Archives of Psychiatry and Clinical Neuroscience},
