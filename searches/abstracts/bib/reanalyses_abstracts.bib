Title = {Analysis of the big-five personality factors in terms of the PAD temperament model.},
Volume = {48},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1998-04928-004&site=ehost-live},
Year = {1996},
}


@article{1996-06275-00119960801,
Abstract = {Structure equation models were applied to reanalyze the data of the 1969 Westinghouse Head Start evaluation. Unlike previous reanalyses, in which structural equation models were only applied to data of the 1st grade Whites in summer programs, this study extended the analyses to the most policy-relevant sections of the full data set, i.e. Blacks and full-year programs. The results indicated that the selection bias found for summer Whites turned out to be absent for Blacks and full-year Whites. Full-year Head Start was effective for Blacks. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
