@article{1982-04984-00119810501,
Abstract = {In Exp I with 24 female albino rats, olfactory cues from prior Ss in operant chambers were shown to be an effective stimulus that could be used by other Ss to direct lever selection in a typical operant drug discrimination (DD) paradigm. Such cues persisted for long periods of time (16 hrs), and were deposited after short (5 min) operant sessions. In extinction tests, interanimal olfactory cues exerted strong stimulus control over lever selection. Furthermore, such cues were not specific to individual Ss but were generalizable between Ss. Interanimal cues directing lever selection could be abolished by cleaning operant manipulanda with a 10% alcohol solution. Reanalysis of some DD data previously reported by the 2nd author (see record [rid]1978-02962-001[/rid]) indicates that this specific earlier study might have been confounded by interanimal cues. Exp II with 16 male albino rats compared the relative potency of nicotine and an olfactory cue to determine whether olfactory or drug cues are prepotent when both are available. It was found that the drug cue was antagonized by mecamylamine for all Ss except those who had a reliable olfactory cue from prior Ss to direct lever selection (Ss who possessed both an olfactory and a drug cue to direct lever selection responded in a way suggesting that the exteroceptive olfactory cue controlled behavior rather than the interoceptive drug cue). Findings indicate that interanimal olfactory cues could be of considerable methodological significance in DD studies. (29 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Extance, K. and  Goudie, A. J.},
ISSN = {0033-3158, 1432-2072},
Journal = {Psychopharmacology},
Keywords = {interanimal olfactory vs nicotine cues, operant drug discrimination, female rats, Cues, Discrimination Learning, Mecamylamine, Nicotine, Olfactory Stimulation, Drugs, Rats, Stimulus Control, Stimulus Generalization},
Number = {4},
Pages = {363 - 371},
Title = {Inter-animal olfactory cues in operant drug discrimination procedures in rats.},
Volume = {73},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1982-04984-001&site=ehost-live},
Year = {1981},
}


