@article{1983-09045-00119821001,
Abstract = {Previous studies aimed at testing the validity of J. L. Holland's (1973) hexagonal and A. Roe's (1956) circular models of interests showed that the null hypothesis of random arrangement can be rejected, and the investigators concluded that the tested models were supported. None of these studies, however, tested each model in its entirety. The present study is based on the assumption that the rejection of the null hypothesis of chance is not rigorous enough. Reanalysis of 13 data sets of published studies, using a more rigorous method, revealed that although the random null hypothesis can in fact be rejected in 11 data sets, the hexagonal–circular model was supported by only 2 data sets and was rejected by 11 of them. The hierarchical model for the structure of vocational interests proposed by the present author (1979) was submitted to an identical test and was supported by 6 of 10 data sets, including 4 that rejected the hexagonal–circular model. The predictions of each of the models that tend to be disconfirmed by empirical data are identified. Implications for the structure of interests and occupational choice are discussed. (33 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Gati, Itamar},
ISSN = {0001-8791},
Journal = {Journal of Vocational Behavior},
Keywords = {reanalysis of previous data supporting hexagonal vs circular vs hierarchical models of vocational interests, Models, Occupational Interests, Statistical Validity},
