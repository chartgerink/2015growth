@article{1984-25883-00119830101,
Abstract = {Reviews the situation-response studies that have provided much of the evidence for interactionism in personality psychology. The results of these studies, which have addressed areas such as group participation and anxiety, are tabulated and subjected to a reanalysis. Serious problems are noted with respect to the methodology and analyses of these studies, and detailed criticisms are offered on the more influential ones. It is argued that it is impossible to institute adequate tests of the trait, situationist, and interactionist positions by means of an ANOVA because several different outcomes are compatible with some or all of the positions. It is concluded that the variance component technique has produced more confusion than clarity and that many of the conclusions concerning interactionism are unwarranted. (55 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Furnham, Adrian and  Jaspars, J.},
