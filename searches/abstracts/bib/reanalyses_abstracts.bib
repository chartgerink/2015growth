ISSN = {1069-3971, 1552-3578},
Journal = {Cross-Cultural Research: The Journal of Comparative Social Science},
Keywords = {cross cultural differences in games, societies, replication, Cross Cultural Differences, Culture (Anthropological), Games},
Number = {2},
Pages = {185 - 206},
Title = {Games in culture revisited: A replication and extension of Roberts, Arth, and Bush (1959).},
Volume = {32},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1998-02280-004&site=ehost-live},
Year = {1998},
}


@article{1998-02767-00519980401,
Abstract = {The Frost Multidimensional Perfectionism Scale (FMPS) provides 6 subscales for a multidimensional assessment of perfectionism: Concern over Mistakes (CM), Personal Standards (PS), Parental Expectations (PE), Parental Criticism (PC), Doubts about actions (DA), and Organization (OR). Despite its increasing popularity in personality and clinical research, the FMPS has also drawn some criticism for its factorial instability across samples. The present article argues that this instability may be due to an overextraction of components. Whereas all previous analyses presented 6-factor solutions for the FMPS items, a reanalysis with Horn's parallel analysis suggested only 4 or 5 underlying factors. To investigate the nature of these factors, item responses from 243 Ss were subjected to principal component analysis. Again, parallel analysis retained only 4 components. Varimax rotation replicated PS and OR as separate factors, whereas combining CM with DA as well as PE with PC. Consequently, the present article suggests a reduction to 4 (instead of 6) FMPS subscales. Differential correlations with anxiety, depression, parental representations and action tendencies underscore the advantage of this solution. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Stöber, Joachim},
ISSN = {0191-8869},
Journal = {Personality and Individual Differences},
Keywords = {4 vs 6 factor solution of Frost Multidimensional Perfectionism Scale, college students, Factor Structure, Perfectionism, Personality Measures, Rating Scales},
Number = {4},
Pages = {481 - 491},
Title = {The Frost Multidimensional Perfectionism Scale revisited: More perfect with four (instead of six) dimensions.},
Volume = {24},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1998-02767-005&site=ehost-live},
Year = {1998},
}


@article{1999-00511-00119980301,
Abstract = {Involuntary autobiographical memories recorded in a diary study (D. Berntsen, 1996) are compared to voluntary autobiographical memories retrieved in response to verbal cues in a laboratory. Cued recall was used to elicit voluntary episodic memories from 12 20–27 yr old students using verbal cues that were comparable to the cues that elicited the involuntary memories. The findings demonstrate that voluntary and involuntary retrieval may access different samples of autobiographical memories. The voluntary memories were less specific, more frequently rehearsed, and less emotionally positive than the involuntary memories. A reanalysis of the diary study examined conditions of involuntary retrieval. It occurred most frequently when attention was diffuse and was typically triggered by environmental cues matching central features of the remembered event. The findings are discussed in relation to current models of autobiographical memory. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Berntsen, Dorthe},
ISSN = {0965-8211, 1464-0686},
Journal = {Memory},
Keywords = {voluntary vs involuntary retrieval of autobiographical memories, 20–27 yr olds, Autobiographical Memory, Human Information Storage},
Number = {2},
