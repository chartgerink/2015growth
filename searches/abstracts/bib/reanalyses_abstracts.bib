Journal = {Social Cognition},
Keywords = {Implicit Association Test, cognitive skill confound, cognitive ability, information processing speed, category congruency, Associative Processes, Cognitive Ability, Cognitive Processes, Cognitive Processing Speed, Stimulus Similarity, Attitude Measurement},
