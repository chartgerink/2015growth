@article{1976-02480-00119750801,
Abstract = {Conducted 3 studies to investigate the generality of the labeling bias effect (i.e., when Ss judge normal individuals as mentally ill as a function of diagnostically irrelevant stimuli). Exp I failed to find the effect with 60 undergraduates but demonstrated that these Ss can discriminate an audiotape recording of a psychiatric patient from a tape of a normal person. Exp II demonstrated labeling bias in 45 mental health service workers. Exp III produced labeling bias in 100 undergraduates by manipulating the "relevance" of the prestige figure. Data indicate that, contrary to previous interpretation, professional identity is not a necessary condition for producing labeling bias. Reanalysis of the data of previous research supported the hypothesis that labeling bias is extremely general and probably related to attractiveness of the communicator who gives the suggestion of mental illness, setting, ambiguity of desired response, and other similar variables in the social psychology literature. (26 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Sushinsky, Leonard W. and  Wener, Richard},
ISSN = {0022-3018, 1539-736X},
Journal = {Journal of Nervous and Mental Disease},
