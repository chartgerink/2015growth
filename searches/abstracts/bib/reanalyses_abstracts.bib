Keywords = {statistical regression toward mean, practice effect between 1st & 2nd ratings, children evaluated for hyperactivity, Hyperkinesis, Medical Diagnosis, Practice, Statistical Regression},
Number = {3},
Pages = {393 - 396},
Title = {Hyperactivity ratings: Statistical regression provides an insufficient explanation of practice effects.},
