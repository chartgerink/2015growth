@article{1997-41261-01319970901,
Abstract = {Recent studies of the mathematical relationship between time and forgetting suggest that it is a power function rather than an exponential function, a finding that has important theoretical consequences. Through computational analysis and reanalyses of published data, we demonstrate that arithmetic averaging of exponential curves can produce an artifactual power curve, particularly when there are large and systematic differences among the slopes of the component curves. A series of simulations showed that the amount of power artifact is small when the slopes of the component curves are normally or rectangularly distributed and when the performance measure is noise free. However, the simulations also showed that the artifact can be quite large, depending on the shape of the noise distribution and restrictions in the performance range. We conclude that claims concerning the form of memory functions should consider whether the data are likely to contain artifact caused by averaging or by the presence of range-restricted noise. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Anderson, Richard B. and  Tweney, Ryan D.},
ISSN = {0090-502X},
Journal = {Memory & Cognition},
Keywords = {averaging of exponential curves in forgetting, power curve artifacts in simulations & analysis of published data, Forgetting, Graphical Displays, Statistical Analysis, Statistical Power},
Number = {5},
Pages = {724 - 730},
Title = {Artifactual power curves in forgetting.},
Volume = {25},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1997-41261-013&site=ehost-live},
Year = {1997},
}


@article{1998-01405-00119970901,
Abstract = {Reassesses results from J. N. Washburne's (1927) study with junior high school students investigating the effect on information recall of data (originally 15 forms of presentation) presented as text, tables, horizontal bar graphs, pictograms, or line graphs. It is stated that although Washburne's conclusions have intuitive appeal and were quoted in the literature on comparative displays, a reanalysis of the results indicates that only the claim that tables are best for recalling specific values is clearly supported by the data. Additional findings of the study, such as the importance of the logical ordering of items in a display, were not acknowledged in subsequent studies. It is concluded that the possible conclusions from the study concern 2 different levels of analysis, providing (1) relevant information at the level of understanding the relative efficacy of different displays for information recall, and (2) insights into the acquisition of knowledge in the field of human factors (i.e., the examination of existing naive preconceptions which can impede this acquisition). (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
