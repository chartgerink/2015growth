@article{1978-24608-00119771001,
Abstract = {Three studies using 200 undergraduates examined the effect of contingent monetary reward on the occurrence of behavior presumably determined by intrinsic task motivation. A general hypothesis that contingent monetary rewards would reduce the frequency of such behaviors was derived from a postulate of cognitive evaluation theory. For all studies, intrinsic task motivation was defined as the amount of free time a S spent working on a task without receiving reward. Study 1 investigated the effects of reward contingency and magnitude on the behavioral measure of intrinsic motivation and on related attitudinal variables. Study 2 examined the effects of reward contingency and 2 individual different variables (self-esteem and perceived locus of control) on intrinsic motivation and attitudinal measures. Although Study 2 showed no significant results, Study 1 demonstrated support for the hypothesis with the behavioral measure of intrinsic motivation, but not with the attitudinal measures. Further examination of the behavioral measure of intrinsic motivation suggested that this variable was bimodally distributed in both Studies 1 and 2. A test for normality indicated that the distributions were significantly nonnormal and that parametric analyses of mean differences were inappropriate. Study 3 reported the results of nonparametric reanalyses of Studies 1 and 2 and of data previously published by E. L. Deci as supportive of the postulate under examination. Deci's data were also found to be generally bimodal. The nonparametric analyses were nonsignificant for all data sets. Theoretical implications are discussed. (36 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Farr, James L. and  Vance, Robert J. and  McIntyre, Robert M.},
ISSN = {0030-5073},
Journal = {Organizational Behavior & Human Performance},
Keywords = {monetary reward contingency & magnitude & locus of control & self esteem, intrinsic task motivation & attitudinal variables toward task, college students, Attitudes, Internal External Locus of Control, Intrinsic Motivation, Monetary Rewards, Reinforcement Amounts, Self Esteem},
Number = {1},
Pages = {31 - 53},
Title = {Further examinations of the relationship between reward contingency and intrinsic motivation.},
Volume = {20},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1978-24608-001&site=ehost-live},
Year = {1977},
}


