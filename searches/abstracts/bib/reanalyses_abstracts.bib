Year = {1995},
}


@article{1996-92865-00119950901,
Abstract = {Describes a model of temporal and neurotopological structure of syntactic processes during comprehension. In Phase 1, parser assigns the initial syntactic structure on the basis of word category information. In Phase 2, lexical-semantic and verb-argument structure information is processed and is neurophysiologically manifest in a negative component in the event-related brain potential around 400 msec after stimulus onset that is distributed based on type of information processed. In Phase 3, the parser tries to map the initial syntactic structure onto the available lexical-semantic and verb-argument structure information. In case of an unsuccessful match between the 2 types of information, structural reanalyses may become necessary, which are correlated with a centroparietally distributed late positive component in the event-related brain potential. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Friederici, Angela D.},
