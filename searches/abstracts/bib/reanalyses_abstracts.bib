Keywords = {adolescence, intercourse, sexual debut, mass media, sexuality, media effects, sexual behavior, television, adolescent, adolescents, media, sexual socialization, Mass Media, Psychosexual Behavior, Psychosexual Development, Sexual Intercourse (Human), Sexuality},
Number = {2},
Pages = {582 - 584},
Title = {Premature dissemination of advice undermines our credibility as scientists: Reply to Brown (2011) and to Collins, Martino, and Elliott (2011).},
URL = {laurence.steinberg@temple.edu},
Volume = {47},
Year = {2011},
}


@article{2011-04453-00120110301,
Abstract = {Longitudinal research has demonstrated a link between exposure to sexual content in media and subsequent changes in adolescent sexual behavior, including initiation of intercourse and various noncoital sexual activities. Based on a reanalysis of one of the data sets involved, Steinberg and Monahan (2011) have challenged these findings. However, propensity score approaches—especially the version of this method used by Steinberg and Monahan, which lacks covariates—do not necessarily result in more accurate estimates of treatment effects than does the regression with covariates approach employed by prior research. There are also a number of problems with the specific set of analyses presented by Steinberg and Monahan and the conclusion they draw from them. In contrast to Steinberg and Monahan's claim, there is substantial evidence of an association between sexual media exposure and adolescent sexual initiation. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Collins, Rebecca L. and  Martino, Steven C. and  Elliott, Marc N.},
ISSN = {0012-1649, 1939-0599},
Journal = {Developmental Psychology},
Keywords = {adolescence, media effects, sexual behavior, television, adolescent, intercourse, mass media, sexuality, Mass Media, Psychosexual Behavior, Psychosexual Development, Sexual Intercourse (Human), Sexuality},
Number = {2},
Pages = {577 - 579},
Title = {Propensity scoring and the relationship between sexual media and adolescent sexual behavior: Comment on Steinberg and Monahan (2011).},
URL = {collins@rand.org},
Volume = {47},
Year = {2011},
}


@article{2008-06150-00620070901,
Abstract = {Comments on an article by Xiang Wan and Paul Pavlidis (see record [rid]2008-06150-005[/rid]). The authors present an excellent overview of efforts to make the reanalysis of microarray data a reality. As Wan and Pavlidis demonstrate, reanalysis of microarray data in neuroscience has not yet occurred to any significant extent. To understand why, it helps to examine the chain of events that constitute this endeavor. At the risk of oversimplification, I have divided this chain into five major events: sample collection, gene-expression profiling, annotation, sharing, and statistical analysts. Given the current state of computer science, there is little evidence that software capable of parsing a biomedical ontology to specify an appropriate analysis will appear on the horizon anytime soon, even for rigorously annotated data. I agree with the authors that software capable of combining a variety of microarray data sets would be an important step toward realizing the full potential of such experiments, and I share their desire to realize this goal. However, I strongly believe that, for the near future, we must emphasize the centrality of interdisciplinary collaboration in the reanalysis of microarray data, rather than promise the development of software supporting independent analysis, as the latter will be a long time in coming. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Herskovits, Edward H.},
ISSN = {1539-2791, 1559-0089},
Journal = {Neuroinformatics},
Keywords = {gene expression, profiling data, neurosciences, neuroscientists, Databases, Gene Expression, Genes, Neurosciences, Data Collection},
Number = {3},
Pages = {176 - 177},
Title = {Software development vis à vis collaboration in interdisciplinary research.},
URL = {ehh@ieee.org},
Volume = {5},
Year = {2007},
}


@article{2012-27172-00120120919,
Abstract = {Comments on an article by Z. G. Zhang et al. (see record [rid]2012-18575-016[/rid]). The goal of triplet paradigm is to identify key-evoked potential parameters that are less affected by stimulus energy and repetition and more strongly correlated with the subject’s perception of pain intensity. According to a recent study by Zhang et al. this is where gamma band oscillations (GBOs) might play a pivotal role in pain research. Zhang et al. reexamined data from the original triplet laser stimulation study with a focus on GBOs. The relationship between GBOs, stimulus repetition, and stimulus intensity is perhaps best illustrated in the left panel. While a relationship emerges between rating of perceived stimulus intensity, LEPs, and GBOs, only GBOs are unaffected by stimulus repetition. That is, GBOs do not appreciably change from one stimulus to the next, and thus, correspond with the rating of perceived intensity to a given stimuli. In the subsequent figures by Zhang et al., this point is illustrated and substantiated by statistical analysis revealing that the intensity of pain but not stimulus repetition or stimulus energy, resulted in significant changes in GBO magnitude. While Zhang et al. were not the first to identify the relationship between GBOs and pain intensity, the reanalysis of the triplet laser stimulation data controls for the potential confound of stimulus saliency. Given the marked effect of saliency on LEPs, it was important to test whether GBOs behave similarly. In contrast to LEP amplitude, laser-induced GBOs appear to represent a more direct measure of pain perception. To account for eye movements, Zhang et al. used independent component analysis to remove ocular artifacts. However, to further minimize the contribution of ocular artifacts, future experiments that aim to study pain-evoked GBOs should consider recently developed guidelines. In the context of clinical trials, GBOs may be useful to objectively monitor and track pain relief due to a therapeutic intervention. Monitoring continuous GBOs may also improve the objective assessment of pain. Such a technique could be equally or potentially more meaningful than event-related GBOs to diagnose spontaneous or idiopathic pain. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
