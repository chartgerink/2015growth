Number = {1},
Pages = {23 - 52},
Title = {Reanalysis of clause boundaries in Japanese as a constraint-driven process.},
URL = {miyamoto@alum.mit.edu},
Volume = {46},
Year = {2003},
}


@article{2003-08124-00920030101,
Abstract = {My colleagues and I published a series of three articles (1994, 1995, and 1996) on the assessment of object relations in abused children. All three studies used the Social Cognition and Object Relations Scales to score Ss' responses to the Thematic Apperception Test. The data on which these articles were based were extracted from an archive that contained social history and psychological testing information on various clinical samples of children and their families. Members of my research team had access to these data for degree-related research projects. Recently, I became aware that there was not an adequate system of checks in place to prevent the reuse of data for some of these projects. This oversight resulted in participant overlap among the samples reported in these studies. The sample overlap in these studies means that the results are not truly independent. To correct and clarify the implications of these errors, this article offers a reanalysis of the data for the unique Ss included in the three original articles. Results of the reanalysis are consistent with the conclusions derived from the original articles, indicating that childhood sexual and physical abuse are associated with object relations impairments in girls from an outpatient community mental health setting. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Ornduff, Sidney R.},
ISSN = {0022-3891, 1532-7752},
