Title = {Championship pressures: Choking or triumphing in one's own territory?},
Volume = {68},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1995-27389-001&site=ehost-live},
Year = {1995},
}


@article{1996-90637-00119950301,
Abstract = {Posits that irony, viewed as a mode of indirect negation, does not involve canceling the indirectly negated message and replacing it with the implicated one. Irony understanding involves processing both the negated and implicated messages, so that the difference between them may be computed. This view differs from the view that assumes that irony involves only 1 interpretation. Holding that irony activates both the literal/explicit and the ironic/implicated meanings predicts that irony will be more difficult to understand than a nonironic use of the same utterance. Reanalysis of previous findings by R. W. Gibbs (see record [rid]1986-16845-001[/rid]) supports this theory. Speakers apply irony for certain communicative goals that are unattainable by direct negation. Direct negation cannot point to the occasionally more desirable state of affairs indicated by the affirmative (literal) phrasing of the ironic utterance. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Giora, Rachel},
ISSN = {0163-853X, 1532-6950},
Journal = {Discourse Processes},
Keywords = {irony as indirect negation & contribution to differences in processing difficulty of ironic vs nonironic language, Figurative Language, Verbal Comprehension, Verbal Meaning},
Number = {2},
Pages = {239 - 264},
Title = {On irony and negation.},
Volume = {19},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1996-90637-001&site=ehost-live},
Year = {1995},
}


@article{1996-32735-00119950301,
Abstract = {Assessed the Paranormal Belief Scale (PBS; J. Tobacyk and G. Milford, see record 71:00249 and Tobacyk, 1988) which measures Traditional Religion, Psi Belief, Precognition, Spiritualism, Witchcraft, Extraordinary Life Forms, and Superstition. The author contends that the present form of the PBS is based on questionable methodology, and even though the factorial structure of paranormal belief is likely to be multidimensional, it cannot be accounted for by a seven-factor simple structure, nor is it likely to be orthogonal. Reconstruction and reanalysis of the original 13-factor solution show that there are more likely to be 4 significant factors. An in-depth review of the PBS subscales shows that subscale items lacked comprehensive coverage of their constructs and are often wrongly named. It also sheds light on why the factor structure may have turned out as it did. A new PBS and a more appropriate definition of paranormal are required. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Lawrence, Tony R.},
ISSN = {0022-3387},
Journal = {Journal of Parapsychology},
Keywords = {factor analysis & construct validity of Paranormal Belief Scale, college students, implications for personality & adjustment, commentary, Attitude Measures, Factor Analysis, Parapsychological Phenomena, Rating Scales, Statistical Validity, Personality, Questionnaires},
