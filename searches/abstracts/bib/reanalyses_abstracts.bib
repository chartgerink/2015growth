ISSN = {1053-8100},
Journal = {Consciousness and Cognition: An International Journal},
Keywords = {contingency learning, color perception, visual discrimination, covariation learning, instance memory, event files, connectionist models, implicit learning, memory resources, storage, retrieval, unlearning, Color Perception, Learning, Operant Conditioning, Visual Discrimination, Human Information Storage, Implicit Learning, Memory},
