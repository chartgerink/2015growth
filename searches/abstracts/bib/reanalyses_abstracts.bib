@article{1996-08008-00119950701,
Abstract = {Comments on R. A. Bartsch and C. M. Judd's (see record [rid]1994-09408-001[/rid]) argument that outgroup homogeneity (OH) effects occur independently of any tendency for members of minority groups to see their ingroup as more homogeneous than the majority outgroup. This argument is based on evidence of an underlying OH effect in a study that purports to unconfound roles of judged group size and ingroup-outgroup judgment by presenting Ss first with a small or large ingroup (or outgroup), then a small comparison outgroup (or ingroup). However, self-categorization theory (SCT) predicts that when an ingroup is judged first it should be perceived as relatively heterogeneous due to the intragroup nature of judgmental context. Consistent with SCT, this reanalysis suggests that manifestations of OH are not independent of contextual factors that determine the relative appropriateness of category-based perception of ingroup and outgroup. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Haslam, S. Alexander and  Oakes, Penelope J.},
