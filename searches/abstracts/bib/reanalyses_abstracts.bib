@article{1997-07795-00319970101,
Abstract = {In a reanalysis of women's language, J. Holmes (1995) has argued that women's use of hedges expresses interpersonal warmth and not, as many researchers have maintained, linguistic tentativeness. It is typically men, she suggests, who employ hedges to convey imprecision and incertitude. This study investigated the use of the hedges sort of and you know in a sample of 50 male and 54 female White South African undergraduates (aged 18–32 yrs). Holmes 's method of analysis was applied to hedging behavior in 52 dyadic conversations. The study consisted of a 2 (Speaker Gender: Male/Female)  × 2 (Audience Gender: Male/Female)  × 2 (Condition: Competitive/Noncompetitive) between-Ss experimental design. Results show that contextual influences eclipsed the effects of gender; in fact, no main effects were found for speaker gender. Fewer hedges were deployed in the competitive condition than in the noncompetitive condition. Moreover, perhaps reflecting differences in social status, both sexes used sort of to express tentativeness more frequently when talking to male addressees. When speaking to female addressees, men deployed facilitative you know hedges more readily than women. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Dixon, John A. and  Foster, Don H.},
ISSN = {0090-6905, 1573-6555},
Journal = {Journal of Psycholinguistic Research},
Keywords = {use of word hedging devices in same- vs mixed-sex conversations, male vs female 18–32 yr olds, South Africa, Conversation, Human Sex Differences, Words (Phonetic Units)},
Number = {1},
Pages = {89 - 107},
Title = {Gender and hedging: From sex differences to situated practice.},
