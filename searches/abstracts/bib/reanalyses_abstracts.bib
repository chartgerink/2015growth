URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1984-11439-001&site=ehost-live},
Year = {1984},
}


@article{1984-10901-00119830801,
Abstract = {Tested 2 explanations of leadership. The 1st explanation, termed the Average Leadership Style, holds that superiors relate uniformly to subordinates in a work group on a given dimension of leadership style and that subordinate's reports of a superior's style are homogeneous across a work group. The 2nd explanation, termed the Vertical Dyad Linkage, focuses on the individualized dyadic exchanges between a superior and each subordinate in a work group, and holds that leaders behave differently toward subordinates in the same work groups on a given dimension. A within- and between-groups analysis was performed on longitudinal data originally compiled by F. Dansereau et al (1975) based on interviews with 17 superiors and 60 subordinates employed in a university housing complex. The key scale employed in the original study—negotiating latitude—attempted to assess the degree to which superiors allowed subordinates to shape a subordinate's role on the job. This reanalysis raises question about the negotiating latitude scale because the results are more compatible with an inference of ambiguous data rather than with an Average Leadership Style or a Vertical Dyad Linkage approach to leadership. It is concluded that within- and between-groups analysis is not limited to further research but can also be used to re-examine assumptions made in previous research. (22 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Nachman, Sidney and  Dansereau, Fred and  Naughton, Thomas J.},
ISSN = {0033-2941, 1558-691X},
Journal = {Psychological Reports},
Keywords = {negotiating latitude as construct in leadership style, supervisors & their subordinates, implications for vertical dyad linkage theory of leadership, Leadership Style, Management Personnel, Interpersonal Influences},
Number = {1},
Pages = {171 - 177},
Title = {Negotiating latitude: A within- and between-groups analysis of a key construct in the vertical dyad linkage theory of leadership.},
Volume = {53},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1984-10901-001&site=ehost-live},
Year = {1983},
}


