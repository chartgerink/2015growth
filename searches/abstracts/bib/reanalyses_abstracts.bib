Keywords = {urine specimens, drug use, adult male arrestees, efficacy of instant urine technology, criminal justice supervision, Drug Usage Screening, Male Criminals, Technology, Urinalysis},
Number = {3},
Pages = {325 - 329},
Title = {A comparison of the Ontrak Testcup-5 to laboratory urinalysis among arrestees.},
