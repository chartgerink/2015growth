@article{1986-05221-00119850401,
Abstract = {Argues that the failure of many complex social interventions can be attributed to a lack of comprehension on the part of respondents with respect to the appropriate behavioral response. The means by which such "comprehension artifacts" can be detected and offset are detailed, using data obtained from a large-scale electric rate demonstration experiment in which 282 treatment group Ss' rates were tied to the time of day at which they used electricity. Standard analytic approaches suggested no treatment influence on Ss' usage behavior. However, reanalysis disclosed that the rate variation manipulation had a considerable impact on those who could be identified as having understood the complex features of the treatment. A methodology is described for discriminating knowledgeable from nonknowledgeable respondents, and the manner in which this categorization can be validated in secondary analysis settings is outlined. The use of the empirically derived categorization as a "blocking" variable is discussed, along with a consideration of alternative methodologies. In research involving complex social interventions, the suggested approach can provide information of considerably greater precision than that obtained through more traditional analytic techniques. (18 ref) (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Crano, William D. and  Messé, Lawrence A.},
ISSN = {0193-841X, 1552-3926},
Journal = {Evaluation Review},
Keywords = {methodology of social interaction research, comprehension artifacts, treatment group Ss, Experimentation, Methodology, Social Psychology},
Number = {2},
Pages = {144 - 172},
Title = {Assessing and redressing comprehension artifacts in social interaction research.},
Volume = {9},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1986-05221-001&site=ehost-live},
Year = {1985},
}


@article{1986-04712-00119851201,
Abstract = {Questions the emphasis on the individual alcoholic as victim of the addiction and suggests that alcoholism literature has treated Alcoholics Anonymous (AA) as an organization central to social policy on alcoholism. The organization has been the hero or villain of these critiques, and AA has been damaged and become the victim of its linkage with other organizations. Studies that have suggested how indigenous persons and groups are co-opted through their employment as paraprofessionals are cited in support of this contention. A reanalysis of the Rand Report (D. Armor et al, 1978) data related to the differential effectiveness of AA and professional treatment is presented. Results suggest that AA and its members were denied a major victory concerning the organization's role in obtaining abstinence as a treatment outcome. It is concluded that there is internal fragmentation in the various traditions identified in social thought related to alcoholism. It is contended that if self-help groups like AA are to continue to exist as autonomous entities, public policy must encompass the disease approach, one that allows alcoholics to assume responsibility for alcoholism without involvement in policy level decisions. (26 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Fry, Lincoln J.},
ISSN = {0022-0426},
Journal = {Journal of Drug Issues},
Keywords = {view of Alcoholics Anonymous as central to social policy on alcoholism & criticism of emphasis on individual alcoholic as victim, Alcoholics Anonymous, Alcoholism},
Number = {1},
Pages = {135 - 146},
Title = {Social thought, social movements and alcoholism: Some implications of AA's linkages with other entities.},
