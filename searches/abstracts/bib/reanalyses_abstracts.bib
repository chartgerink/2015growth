Title = {Double-blind peer review and gender publication bias.},
URL = {lengqvist@evolution.uni-bonn.de},
Volume = {76},
Year = {2008},
}


@article{2008-09327-00420080701,
Abstract = {Comments on an article by Jim McCambridge (see record [rid]2007-12973-001[/rid]). McCambridge presents a case study of publication bias in reviews of school-based drug education programmes. He reports that a reanalysis of 207 studies of the effects of such programmes resulted in different conclusions than those derived from previous meta-analyses. McCambridge points to several important aspects of such a publication bias, both with respect to how we may explain its occurrence and with respect to implications for policy and practice. Publication bias is of particular concern due to its potential consequences for policy and practice in the field. McCambridge points to the need for unbiased evidence on drug prevention in schools. In the Norwegian case it seems likely that the published evidence of effectiveness of the Youth & Alcohol programme has been a key factor for the recommendations of the programme by public health bodies. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
