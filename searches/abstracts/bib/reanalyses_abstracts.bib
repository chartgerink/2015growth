}


@article{2000-08540-01420000701,
Abstract = {Increases in category access (CA) and items recalled per category (IPC) are associated with increases in relational and item-specific processing, respectively. However, it has also been shown that CA increases as recall level increases and that CA scores following relational processing are actually below CA scores for randomly recalled items. These results prompted M. D. Murphy (1979) to suggest that, after adjusting for recall-level differences, relational processing decreases CA scores. Results of Experiment 1, along with a reanalysis of previously published data, showed that relational processing produces lower CA scores than purely item-specific processing (or random recall), but an increase in relational processing produces an increase in CA scores even when the CA and IPC scores are adjusted for recall-level differences. These results suggest a curvilinear relationship between relational processing and CA scores. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Burns, Daniel J. and  Brown, Cynthia A.},
ISSN = {0278-7393, 1939-1285},
Journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
Keywords = {relational vs item-specific processing & category access scores, adults, Classification (Cognitive Process), Cognitive Processes, Recall (Learning)},
Number = {4},
Pages = {1057 - 1062},
Title = {Observations: The category access measure of relational processing.},
Volume = {26},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2000-08540-014&site=ehost-live},
Year = {2000},
}


@article{2007-09402-00420000601,
Abstract = {One cannot seriously discuss the scientific study of couple therapy without mentioning Neil Jacobson. His body of research was a major part of the empirical foundation on which the scientific community judged behavioral couple therapy (or behavioral marital therapy) as the most widely researched treatment for couples and as the only couple treatment reaching the highest level of empirical support. Although Neil did not invent the concept of clinically significant change, he was the first to operationally define it in statistical terms. In the article Variability in outcome and clinical significance of behavioral marital therapy: A reanalysis of outcome data (see record [rid]1984-32021-001[/rid]), Neil and his colleagues developed a metric that has gained widespread acceptance in the broader area of outcome research. Yet Neil's lasting contribution is not just the development of a metric for assessing clinical significance, but also the emphasis he put on clinical significance. Neil's work not only caught the attention of the scientific community, it changed it in important ways. His work changed how we do therapy and how we evaluate therapy. Without a doubt, his work was clinically significant. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Christensen, Andrew},
ISSN = {1522-3736},
Journal = {Prevention & Treatment},
Keywords = {effectiveness of behavioral marital therapy, reanalysis of data from outcome studies, Couples Therapy, Marriage Counseling, Treatment Outcomes},
Number = {1},
Title = {The clinical significance of Neil's couple therapy research.},
URL = {Christensen@psych.ucla.edu},
Volume = {3},
Year = {2000},
}


@article{2007-09402-00320000601,
Abstract = {The 1984 article that Neil Jacobson senior authored, entitled Variability in outcome and clinical significance of behavioral marital therapy: A reanalysis of outcome data (see record [rid]1984-32021-001[/rid]), is now 15 years old. However, it continues to be cited widely in the literature, and it has scientific importance for both methodological and substantive reasons. In addition, embedded in this article are factors that tell us about the attitudes, life, and work style of Neil Jacobson. The creativity exemplified in this work, the scientific rigor and overall high quality, and the quantity of research in this domain of couple outcome research are extraordinary. In addition to the methodological and scientific contributions exemplified in this one study, the multiauthored article tells us something about Jacobson and his approach to psychology. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Baucom, Donald H.},
ISSN = {1522-3736},
Journal = {Prevention & Treatment},
Keywords = {effectiveness of behavioral marital therapy, reanalysis of data from outcome studies, Behavior Therapy, Marriage Counseling, Treatment Outcomes},
Number = {1},
Title = {Neil, you're clinically significant: You did make a meaningful difference.},
URL = {don_baucom@unc.edu},
Volume = {3},
Year = {2000},
}


