@article{1983-33498-00119830301,
Abstract = {There has recently been a great deal of interest in the use of meta-analysis to integrate research findings. G. V. Glass and M. L. Smith (1979) used meta-analytic techniques to describe the relationship between class size and academic achievement or classroom processes. The statistical methods used by Glass and Smith are open to criticism on several grounds. The present paper reports the results of reanalyses using statistical methods that can be rigorously justified. The statistical methods used herein are shown to have certain optimal properties for the analysis of effect size data, and therefore the results of analyses based on these methods are generally preferable. The results of the present analyses suggest that the use of suboptimal statistical methods did not greatly affect the results of the meta-analysis by Glass and Smith. (12 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Hedges, Larry V. and  Stock, William},
ISSN = {0002-8312, 1935-1011},
Journal = {American Educational Research Journal},
Keywords = {meta-analysis, effect of class size on academic achievement & classroom processes, criticism of G. V. Glass & M. L. Smith's statistical methods, Academic Achievement, Class Size, Classroom Environment, Size, Statistical Analysis},
