Keywords = {analysis of variance, pretest-posttest control group design data, Analysis of Variance, Experimental Design},
Number = {4},
Pages = {511 - 518},
Title = {Using a repeated measures ANOVA to analyze the data from a pretest-posttest design: A potentially confusing task.},
Volume = {82},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1975-26619-001&site=ehost-live},
