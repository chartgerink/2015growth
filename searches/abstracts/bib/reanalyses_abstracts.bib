@article{2005-08795-00620050101,
Abstract = {It has been calculated that monogenic genes for X-linked mental retardation (XLMR) account for <10% of male MR. More than 20 genes are known to be associated with syndromic XLMR and 13 genes have so far been identified with nonsyndromic XLMR. Although the search for genes has been very active in recent years, mutations in a "major" causative gene underlying a larger proportion of unexplained XLMR remain to be identified. Recently, the identification of mutations in the Aristaless related homeobox gene (ARX) in syndromic as well as non syndromic conditions created optimism that ARX could be a major gene in XLMR. In the light of the discovery of the ARX gene, reanalysis of data from a population-based study can provide information on the frequency of ARX mutations and other XLMR genes in male MR. In a cross sectional study comprising 30,037 Norwegian children (15,495 males and 14,542 females) born between 1980 and 1985 all children with MR, defined as IQ < 70 after psychometric assessments (n = 185), were identified. The study also included three pairs of brothers, one with possible autosomal or X-linked recessive inheritance and two labeled familial MR. In conclusion, as a crude estimate derived from this study, the prevalence of the 24 bp ARX mutation in male MR is 1%, or less, and is perhaps one-third to half as common as the fragile X syndrome. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Strømme, Petter},
ISSN = {1304-2580},
Journal = {Journal of Pediatric Neurology},
Keywords = {X-linked mental retardation, monogenic genes, Aristaless related homeobox gene, males, Genes, Human Males, Mutations, Intellectual Development Disorder},
Number = {2},
Pages = {121 - 122},
Title = {ARX and other single genes in X-linked mental retardation: Revisiting a population-based study.},
URL = {petter.stromme@medisin.uio.no},
Volume = {3},
Year = {2005},
}


