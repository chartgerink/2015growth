@article{1986-29323-00119860601,
Abstract = {Conducted a conceptual replication of the findings of J. W. Pennebaker et al (see record [rid]1982-27566-001[/rid]) that systolic blood pressure (SBP) changes are correlated with idiosyncratic patterns of symptoms within individuals and attempted to develop a standardized clinical procedure for assessing symptom–BP relationships. 11 normotensive undergraduate males participated in a series of 16 brief tasks. The 1st 8 tasks were mental stressors (Wechsler Adult Intelligence Scale—Revised [WAIS—R] subtests), whereas the 2nd 8 were physical stressors (e.g., breath-holding, running in place). Following each task or baseline, BP was measured and Ss rated the degree to which they were experiencing each of 9 symptoms (e.g., racing heart, sweaty hands). For each S, simple symptom–BP correlations were computed across the 16 measurement periods. High symptom–SBP correlations were obtained only for the physical task period. It is argued that the magnitudes of the correlations reported by Pennebaker et al were spuriously inflated by the inclusion of strenuous physical exercise, and a reanalysis of the data from that study supports this contention. It is concluded that the clinical value of assessing idiosyncratic symptom–BP relationships seems doubtful. (27 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Stewart, Herbert L. and  Olbrisch, Mary E.},
ISSN = {0160-7715, 1573-3521},
Journal = {Journal of Behavioral Medicine},
Keywords = {idiosyncratic symptom patterns, systolic blood pressure, normotensive male college students, replication of findings of J. W. Pennebaker et al, Individual Differences, Symptoms, Systolic Pressure, Experimental Replication},
Number = {3},
Pages = {271 - 289},
Title = {Symptom correlates of blood pressure: A replication and reanalysis.},
Volume = {9},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1986-29323-001&site=ehost-live},
Year = {1986},
}


@article{1986-26853-00119851201,
Abstract = {Reviews the study conducted by J. McV. Hunt and J. Paraskevopoulos (see record [rid]1980-27683-001[/rid]) and argues that their findings that mothers' tendency to overestimate their children's performances on intelligence test items had a strongly negative correlation with children's intellectual ability, whereas their tendency to underestimate had a negligible correlation, were misleading side effects of the method used to measure mothers' tendencies. Appropriate alternative measures are suggested in which the frequency of overestimations is taken as a proportion of opportunities to overestimate (i.e., wrong answers by the child). The higher this proportion, the stronger the mother's tendency to overestimate. The frequency of underestimations is taken as a proportion of opportunities to underestimate (i.e., right answers by the child). The higher this proportion, the stronger the mother's tendency to underestimate. Results that would have been obtained with these measures are approximated. According to this reanalysis, mothers' tendencies to both overestimate and underestimate have modest but significant negative correlations with children's intellectual ability. Mothers' tendency to make accurate estimations had a moderately strong positive correlation with children's intellectual ability. (6 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Price, Gary G. and  Gillingham, Mark G.},
ISSN = {0022-1325, 1940-0896},
Journal = {The Journal of Genetic Psychology: Research and Theory on Human Development},
Keywords = {maternal tendency to overestimate vs underestimate children's intelligence test performance, intellectual ability, children, review of study by J. McV. Hunt & J. Paraskevopoulos, Cognitive Ability, Expectations, Mothers, Parental Attitudes},
Number = {4},
Pages = {477 - 481},
Title = {Effects of mothers' overestimations and underestimations of their children's intellectual ability: A reanalysis of Hunt and Paraskevopoulos.},
Volume = {146},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1986-26853-001&site=ehost-live},
Year = {1985},
}


@article{1986-26411-00119850901,
Abstract = {Distributions of food reinforcement between 2 components of multiple VI schedules were varied over a number of conditions for 5 homing pigeons. Results indicate that sensitivity of reinforcement, measured by the exponent of the power function relating ratios of responses in the 2 components to ratios of reinforcers obtained in the components, did not differ between conditions with 15-sec or 60-sec component durations. The failure to demonstrate the short-component effect, in which sensitivity is high for short components, was consistent with reanalysis of previous data. With 60-sec components, sensitivity to reinforcement decreased systematically with time from component alternation and was higher in the 1st 15-sec subinterval of the 60-sec component than for the component whose total duration was 15 sec. It is concluded that varying component duration and sampling behavior at different times from component transition may not be equivalent ways of examining the effects of average temporal distance between components. (17 ref) (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {White, K. Geoffrey and  Pipe, Margaret-Ellen and  McLean, Anthony P. and  Redman, Selina},
ISSN = {0022-5002, 1938-3711},
Journal = {Journal of the Experimental Analysis of Behavior},
Keywords = {15 vs 60 sec component multiple VI schedules, sensitivity of reinforcement, pigeons, Interstimulus Interval, Reinforcement Schedules, Variable Interval Reinforcement, Pigeons},
Number = {2},
Pages = {207 - 215},
Title = {Temporal proximity and reinforcement sensitivity in multiple schedules.},
Volume = {44},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1986-26411-001&site=ehost-live},
Year = {1985},
}


@article{1986-26308-00119860801,
Abstract = {A reanalysis of part of P. Podgorny and R. N. Shepard's (see record [rid]1984-03057-001[/rid]) data shows that reaction times (RTs) to attended and unattended squares are almost identical when the attended areas are nonunitary and that RTs are related to compactness for unitary attended areas but that this relation breaks down when attention is focused on nonunitary areas. The importance of the actual grid location of probes on RTs is also illustrated. The failure of compactness to reflect these aspects of the spatial nature of attention suggests that this metric is deficient when applied to the study of the spatial determinants of attention. (8 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
