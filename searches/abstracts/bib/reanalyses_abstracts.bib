Year = {1986},
}


@article{1987-00169-00119850101,
Abstract = {Investigated acquiescent response bias in the Wilson Conservatism scale via reanalysis of published data from 2 Japanese samples, 1 New Zealand sample, and 1 US sample. The correlation between liberal and conservation subscales was used as a criterion of construct validity, and it was hypothesized that the 2 Japanese samples would show less validity for the scale than would the 2 Western samples. It also was hypothesized that the New Zealand sample would show the highest validity. Although early studies of acquiescence treated it as a trait, evidence is reviewed indicating that nonmeaningful acquiescence is elicited as a response to some aspect of the test or test situation. Its presence can be detected with balanced scales, which yield a validity breakdown for the particular scale with that particular sample. Such balanced scales can be used to control for acquiescence and also to estimate validity of a particular test administration. Results show that 1 of the Japanese samples and the US sample showed satisfactory validity, whereas the other Japanese sample and the New Zealand sample showed little validity. Acquiescence response bias is virtually unpredictable, but measurements of its effects are vital for determining the validity of a test. (French & German abstracts) (18 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Ray, John J.},
ISSN = {0033-3018},
Journal = {Psychologische Beitrage},
Keywords = {acquiescent response bias in Wilson Conservatism scale & implications for test validity, respondents, Japan vs New Zealand vs US, Conservatism, Personality Measures, Response Bias, Test Validity, Cross Cultural Differences},
