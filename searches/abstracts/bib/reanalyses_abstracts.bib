Title = {A reevaluation of Rescorla's early dictums about Pavlovian conditioned inhibition.},
Volume = {111},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1992-18741-001&site=ehost-live},
Year = {1992},
}


@article{1993-36015-00119920101,
Abstract = {Reanalyzed data from a study by B. M. Byrne (1989; see also PA, Vol 77:16093) to demonstrate that the general model typically used in the confirmatory factor analysis (CFA) approach to the multitrait-multimethod (MTMM) model may lead to inappropriate interpretations even when it converges to a proper solution. The reanalysis suggests that there is reasonable support for the invariance of the MTMM solutions across groups of high school students in high- vs low-ability tracks. Data fit a CFA model positing correlated trait factors and methods as correlated uniquenesses better than the general CFA model positing correlated trait and correlated method (CTCM) factors. The CTCM model was so unstable that it provided many inconsistent solutions that were similar in their ability to fit the data; thus, this model seems to suffer from indeterminacy problems typically associated with exploratory factor analysis. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
