@article{2000-03139-00220000401,
Abstract = {The P600 component in Event Related Potential research has been hypothesized to be associated with syntactic reanalysis processes. The authors, however, propose that the P600 is not restricted to reanalysis processes, but reflects difficulty with syntactic integration processes in general. First the authors discuss this integration hypothesis in terms of a sentence processing model proposed elsewhere. Next, in Exp 1, the authors show that, in 29 18–35 yr old Ss, the P600 is elicited in grammatical, non-garden path sentences in which integration is more difficult (i.e., "who" questions) relative to a control sentence ("whether" questions). This effect is replicated in Exp 2 (which used 36 17–35 yr old Ss). Furthermore, the authors directly compare the effect of difficult integration in grammatical sentences to the effect of agreement violations. The results suggest that the positivity elicited in "who" questions and the P600-effect elicited by agreement violations have partly overlapping neural generators. This supports the hypothesis that similar cognitive processes, i.e., integration, are involved in both 1st pass analysis of "who" questions and dealing with ungrammaticalities (reanalysis). (PsycINFO Database Record (c) 2014 APA, all rights reserved)},
Author = {Kaan, Edith and  Harris, Anthony and  Gibson, Edward and  Holcomb, Phillip},
ISSN = {0169-0965, 1464-0732},
