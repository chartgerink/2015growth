@article{1979-01221-00119780201,
Abstract = {The author's previous analyses (1974, 1976) of teachers' comments in school records of schizophrenics showed progressive deviance in their childhood social behavior and sharp differences between the sexes. The present report presents a replication of the longitudinal study, using 11 female and 5 male schizophrenics and 32 controls, and a reanalysis of the combined samples. Median age at admission of new Ss was 23 yrs. The principal findings from the earlier study were confirmed: In the later school years, preschizophrenic girls were primarily introverted and preschizophrenic boys mainly disagreeable. In the new study, the preschizophrenic girls were also emotionally unstable, introverted, and passive from kindergarten through Grade 6, suggesting an earlier history of social disengagement than was previously apparent, and the preschizophrenic boys were emotionally unstable in Grades 7–22, confirming a nonsignificant trend in the original study. The boys were not behaviorally distinguishable from other boys in elementary grades, so the notion of progressive deviance appears more applicable in their case. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Watt, Norman F.},
ISSN = {0003-990X, 1538-3636},
Journal = {Archives of General Psychiatry},
Keywords = {patterns of childhood social development, adult schizophrenics, Childhood Development, Psychosocial Development, Schizophrenia},
Number = {2},
Pages = {160 - 165},
Title = {Patterns of childhood social development in adult schizophrenics.},
Volume = {35},
