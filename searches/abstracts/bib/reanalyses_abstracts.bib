Keywords = {amygdala activity, music listening, irregular chord functions, emotion, functional MRI, Amygdala, Emotional Content, Music Perception, Magnetic Resonance Imaging, Syntax},
Number = {18},
Pages = {1815 - 1819},
Title = {Amygdala activity can be modulated by unexpected chord functions during music listening.},
