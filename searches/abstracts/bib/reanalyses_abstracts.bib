Pages = {463 - 463},
Title = {Response to article by DeSoto and Hitlan on the relationship between mercury exposure and autism.},
Volume = {23},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2008-04662-020&site=ehost-live},
