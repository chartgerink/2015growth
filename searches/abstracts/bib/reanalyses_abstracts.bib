@article{1995-04395-00119940901,
Abstract = {Argues that the proposition that overprediction of fear is a form of regression toward the mean represents a description (phenomenon), not an explanation. Regression may be due to psychologically meaningful factors and should not be dismissed as a statistical artifact. Arguments against a regression interpretation of overprediction can be supplemented by empirical evidence. A reanalysis of the work of (1) T. M. McMillan and S. J. Rachman (see record [rid]1989-10445-001[/rid]) and (2) S. J. Rachman and S. Taylor (see record [rid]1994-17760-001[/rid]) showed that, in Exp 1, the magnitude of overprediction of the fearless group was as high as that of the overconfident group and higher than that of the courageous group; in Exp 2, Ss in the group making high fear predictions tended to overpredict fears, and Ss with low fear predictions significantly overpredicted. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Taylor, Steven},
ISSN = {0005-7967},
