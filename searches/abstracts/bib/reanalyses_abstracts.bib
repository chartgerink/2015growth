@article{1976-29221-00119760901,
Abstract = {In 3 experiments, a Brown-Peterson task was followed by a final free recall (FFR) of all words. In Exp I, which was a reanalysis of data from a study by V. Modigliani and J. Seamon, the probability of FFR given a prior successful short-term recall, p (FFR/STR), increased with t, the length of the short-term interval. The unconditional probability of FFR, p (FFR), also increased with t. It is argued that the latter result could not be accounted for by item-selection processes alone. Instead, it is necessary to assume that an initial recall strengthened the recalled items and that this strengthening effect increased with t. In Exp II with 30 7th-8th graders, the use of a control group in which no initial recall was required completely verified the above proposition. Exp III, with 24 college students, showed that in this kind of task, there is no one-to-one correspondence between   p (STR) (the probability of initial recall) and either p (FFR/STR) or p (FFR). All data are interpreted in terms of a model which separated item-selection and strengthening effects of the initial recall. (22 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Modigliani, Vito},
ISSN = {0096-1515},
Journal = {Journal of Experimental Psychology: Human Learning and Memory},
Keywords = {delay of initial recall, subsequent recall, 7th & 8th graders & college students, Free Recall, Practice, Short Term Memory},
