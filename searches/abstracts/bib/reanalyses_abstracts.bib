Journal = {Quality of Life Research: An International Journal of Quality of Life Aspects of Treatment, Care & Rehabilitation},
Keywords = {construct validity, Thyroid-Specific Patient Reported Outcome Measure, quality of life, test reliability, Quality of Life, Questionnaires, Test Construction, Thyroid Disorders, Test Reliability, Test Validity},
Number = {4},
Pages = {483 - 496},
Title = {Establishing construct validity for the Thyroid-Specific Patient Reported Outcome Measure (THyPRO): An initial examination.},
