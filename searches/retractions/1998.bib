Number = {1},
Pages = {45 - 51},
Title = {Koro (genital retraction syndrome): A symptom or a disease?.},
Volume = {10},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1998-12777-008&site=ehost-live},
Year = {1998},
