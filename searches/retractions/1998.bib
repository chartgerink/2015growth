ISSN = {0254-4962},
Journal = {Psychopathology},
Keywords = {droperidolum, koro, 29 yr old Asian female, Asians, Culture Bound Syndromes, Drug Therapy, Koro},
Number = {4},
Pages = {220 - 224},
Title = {Genital retraction syndrome in a Korean woman: A case of koro in Hungary.},
Volume = {31},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1998-10257-008&site=ehost-live},
Year = {1998},
}
@article{1998-12777-00819980301,
Abstract = {Koro is a culture-specific psychiatric disorder occurring primarily in southeast Asia. Its cardinal feature is an acute anxiety associated with the fear of genital retraction, accompanied by fear of death induced by a belief that complete disappearance of the genital organ will result in death. Koro has been described in non-Asian patients as well, leading to debate about the culture-specific nature of the syndrome. Giving a psychopathologic, a nosologic definition of koro is also problematic, reflected by the fact that the term "genital retraction syndrome" has become preferable. The authors briefly survey the literature on this syndrome, and report on two cases, one involving a 29-yr-old South Korean woman and one involving a 35-yr-old male patient with a history of paranoid schizophrenia. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
