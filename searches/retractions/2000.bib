@article{2001-00281-00320001201,
Abstract = {Investigated influences on the development and retraction of unusual structures during early bilingual language development, by examining non-target syntactic structures produced by young German–English bilingual children. Four children were studied (audio and video recordings) from roughly 2–4 yrs of age. The results are described as supporting the contentions of the Competition Model that language acquisition proceeds through orientation to surface structures, and that cue competition across languages changes the weighting of cues within the language. The retreat from non-target structures was made possible through the children's attention to contrasts between the languages and the need to resolve structural incompatibilities within the language. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Döpke, Susanne},
ISSN = {1366-7289},
Journal = {Bilingualism: Language and Cognition},
Keywords = {development of & retraction from cross-language structures during bilingual language development, German–English bilingual children studied from ages 2–4, Bilingualism, Code Switching, Language Development, Syntax, Early Childhood Development},
Number = {3},
Pages = {209 - 226},
Title = {Generation of and retraction from cross-linguistically motivated structures in bilingual first language acquisition.},
Volume = {3},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2001-00281-003&site=ehost-live},
Year = {2000},
}
