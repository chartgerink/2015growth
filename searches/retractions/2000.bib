Journal = {Psychopharmacology},
Keywords = {receptor alkylator N-ethoxycarbonyl-2-ethoxy-1, 2-dihydroquinoline, intrinsic activity or efficacy of 5-HT1A agonists & its ability to produce lower-lip retraction, male rats, Drug Interactions, Receptor Binding, Serotonin Agonists, Drug Dosages, Rats},
Number = {4},
Pages = {377 - 387},
Title = {In vivo estimates of efficacy at 5-HT1A receptors: Effects of EEDQ on the ability of agonists to produce lower-lip retraction in rats.},
Volume = {149},
