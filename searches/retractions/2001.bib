Keywords = {functional impairments in speech prosody, left hemisphere damage, right hemisphere damage, aphasic patients, stress production deficits, fundamental frequency, Aphasia, Brain Damage, Lateral Dominance, Prosody, Speech Characteristics, Oral Communication},
Number = {3},
Pages = {482 - 494},
Title = {Production of stress retraction by left- and right-hemisphere-damaged patients.},
Volume = {79},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2002-00169-009&site=ehost-live},
Year = {2001},
}
