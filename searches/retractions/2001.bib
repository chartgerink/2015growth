@article{2001-05556-00820011101,
Abstract = {The pathological mechanism of convergence retraction nystagmus (CRN) is not known. To determine whether CRN is a disorder of vergence or of the saccadic system, the scleral search coil technique was used to record binocularly the three-dimensional components of CRN in a 62-yr-old male patient with a left mesencephalic infarction involving the nucleus of the posterior commissurc and the rostral interstitial nucleus of the medial longitudinal fascicle. CRN had disconjugate horizontal and torsional components. The horizontal amplitude/velocity relationship of CRN aligned with the main sequence of vergence responses of normal control subjects but not with that of saccades. Vergence responses of the right eye and left eye were not asynchronous. The slow phases of CRN showed an exponential decay with a time constant of 70 milliseconds. Thus, CRN is probably a disorder of vergence rather than of opposing adducting saccades. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Rambold, Holger and Kömpf, Derlef and Helmchen, Christoph},
ISSN = {0364-5134},
Journal = {Annals of Neurology},
Keywords = {vergence, convergence retraction nystagmus, saccades, Eye Convergence, Nystagmus},
Number = {5},
Pages = {677 - 681},
Title = {Convergence retraction nystagmus: A disorder of vergence?.},
Volume = {50},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2001-05556-008&site=ehost-live},
Year = {2001},
}
