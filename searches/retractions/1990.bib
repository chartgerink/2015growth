@article{1990-32240-00119900801,
Abstract = {Investigated how in-store price promotions affect market share after the promotions have been retracted. Ss were 129 university students (Exp 1), 54 undergraduates (Exp 2), and 43 female undergraduates (Exp 3). The effects of promotion were contingent on both the choice patterns of Ss (whether or not Ss switched among brands) and the ubiquity of promotions in a product category. If only one brand was being promoted and Ss were generally loyal to the last brand purchased, brand choice probability declined from prepromotion levels once the promotion was withdrawn. However, if Ss tended to switch among brands in the absence of promotions, or if several brands were being promoted, this decline was mitigated and/or did not occur. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Kahn, Barbara E. and Louie, Therese A.},
ISSN = {0022-2437},
Journal = {Journal of Marketing Research},
Keywords = {retraction of in store price promotions, brand choice & market share, college students, Advertising, Brand Names, Choice Behavior, Consumer Behavior, Costs and Cost Analysis},
Number = {3},
Pages = {279 - 289},
Title = {Effects of retraction of price promotions on brand choice behavior for variety-seeking and last-purchase-loyal consumers.},
Volume = {27},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1990-32240-001&site=ehost-live},
Year = {1990},
}
@article{1991-09898-00119900701,
Abstract = {Reports a case of complete Koro complex in a 73-yr-old male suffering from psychotic depression following a nondominant temporoparietal stroke. Koro is comprised of beliefs that (1) the genitals are shrinking, (2) they will disappear into the abdomen, and (3) that this process will result in death. Koro lacks diagnostic and cultural specificity and should be renamed the genital retraction syndrome. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Anderson, D. N.},
ISSN = {0007-1250},
Journal = {The British Journal of Psychiatry},
Keywords = {koro complex, male 73 yr old with psychotic depression after nondominant temporoparietal stroke, case report, Body Image Disturbances, Cerebrovascular Accidents, Major Depression, Male Genitalia, Case Report},
Pages = {142 - 144},
Title = {Koro: The genital retraction symptom after stroke.},
Volume = {157},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1991-09898-001&site=ehost-live},
Year = {1990},
}
