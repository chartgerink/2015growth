Author = {LoLordo, Vincent M. and Ross, Robert T.},
ISSN = {0097-7403},
Journal = {Journal of Experimental Psychology: Animal Behavior Processes},
Keywords = {blocking analysis of Pavlovian occasion setting vs instrumental discriminative stimuli, rats, retraction, Classical Conditioning, Discrimination Learning, Operant Conditioning, Stimulus Discrimination, Rats},
