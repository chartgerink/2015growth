@article{2003-08059-00120030901,
Abstract = {Retracts an article by George A. Ricaurte et al (Science, 2002[Sep], Vol 297[5590], 2260-2263). The authors discovered that the drug used to treat all but one animal in that report came from a bottle that contained (+)-methamphetamine instead of the intended drug, (±)MDMA. (The following abstract of this article originally appeared in record [rid]2002-18729-002[/rid]). Notes that the prevailing view is that the popular recreational drug (+-)3,4-methylenedioxymethamphetamine (MDMA, or "ecstasy") is a selective serotonin neurotoxin in animals and possibly in humans. Nonhuman primates (squirrel monkeys and baboons) were used to evaluate the neurotoxic potential of a dose regimen modeled closely after one used by MDMA users at all-night dance parties. Subjects (Ss) were given MDMA at a dosage of 2 mg/kg, 3 times, at 3 hr intervals, for a total dose of 6 mg/kg. The authors also explored the possibility that monkeys with MDMA-induced dopaminergic neurotoxicity were at an increased risk for },
Author = {Ricaurte, George A. and Yuan, Jie and Hatzidimitriou, George and Cord, Branden J. and McCann, Una D.},
ISSN = {0036-8075},
Journal = {Science},
Keywords = {severe dopaminergic neurotoxicity, methylenedioxymethamphetamine, ecstasy, recreational dose regimen, selective serotonin neurotoxin, nonhuman primates, squirrel monkeys, baboons, motor dysfunction, Dopamine, Drug Dosages, Methylenedioxymethamphetamine, Neurotoxins, Serotonin, Animal Models, Baboons, Drug Usage, Monkeys, Motor Processes, Neurotoxicity, Primates (Nonhuman)},
Number = {5639},
Pages = {1479},
Title = {Severe dopaminergic neurotoxicity in primates after a common recreational dose regimen of MDMA ('ecstasy'): Retraction.},
Volume = {301},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2003-08059-001&site=ehost-live},
Year = {2003},
}