@article{2003-08550-00420030301,
Abstract = {Reports an error in the original article by A. Abu-Akel (Language and Speech, 1997[Oct-Dec], 40[4], 331-351). It is the stated editorial policy of this journal that only original work not previously published elsewhere will be accepted for publication in "Language and Speech". Accordingly, "Language and Speech" retracts the above paper in full, as it has recently been discovered that portions of it were previously published elsewhere by Jonathan Fine. The editors, reviewers, and publisher wish to state that at the time this paper was submitted for publication, it was understood from Mr. Abu-Akel that this was his original work. We very much regret and apologize for any damage caused by the publication of his paper in this journal. (The following abstract of this article originally appeared in record [rid]1998-04206-002[/rid].) Evaluated the communicative deficits in the linguistic performance of 2 Arabic speaking schizophrenics (aged 15 & 16 yrs) having a 1st episode of disorganized s},
Author = {Abu-Akel, Ahmad},
ISSN = {0023-8309},
Journal = {Language and Speech},
Keywords = {communicative deficits, cohesive patterns, dynamic choices, linguistic performance, clozapine treatment, Arabic speaking disorganized schizophrenic patients, Clozapine, Drug Therapy, Linguistics, Oral Communication, Schizophrenia (Disorganized Type), Arabs, Choice Behavior},
Number = {1},
Pages = {ii},
Title = {'A study of cohesive patterns and dynamic choices utilized by two schizophrenic patients in dialog, pre- and post medication.': Retraction.},
Volume = {46},
URL = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2003-08550-004&site=ehost-live},
Year = {2003},
}
